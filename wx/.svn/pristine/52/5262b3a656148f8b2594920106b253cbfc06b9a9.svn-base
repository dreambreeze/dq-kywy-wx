.top-jumpover {
  text-align: right;
  padding: 30rpx 30rpx 15rpx;
}

.jump-overbtn {
  width: 110rpx;
  height: 54rpx;
  border-radius: 4rpx;
  color: #fff;
  display: inline-block;
  font-size: 30rpx;
  text-align: center;
  line-height: 54rpx;
  background-color: #eadecc;
}

.big-title {
  text-align: center;
  font-size: 50rpx;
  font-weight: 500;
  color: #000;
  line-height: 1;
  margin-bottom: 34rpx;
  letter-spacing: 4rpx;
}

.sm-title {
  text-align: center;
  font-size: 32rpx;
  color: #646464;
  margin-bottom: 60rpx;
  line-height: 1;
  letter-spacing: 4rpx;
}

.input-item {
  line-height: 1;
  margin: 0 90rpx;
}

.input-item .flex-wrp {
  padding: 25rpx 0;
}

.input-item:nth-of-type(1) {
  border-bottom: 1rpx solid #f00;
}

.label-title {
  font-size: 40rpx;
  color: #646464;
  flex: 1;
}

.input-con {
  flex: 1;
}

.radio {
  font-size: 28rpx;
  margin-right: 35rpx;
}

.picker {
  font-weight: 300;
  width: 100%;
}

.dc-btn {
  width: 576rpx;
  height: 90rpx;
  line-height: 90rpx;
  color: #fff;
  box-shadow: 0 0 14px rgba(189, 144, 85, 0.6);
  text-align: center;
  border-radius: 3px;
  margin: 100rpx auto 80rpx;
  font-size: 34rpx;
  font-weight: bold;
}

.selfModal {
  width: 600rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  margin-left: -300rpx;
  background-color: #fff;
  border-radius: 6rpx;
  box-shadow: 0 0 20rpx rgba(0, 0, 0, 0.4);
}

.content-modal {
  padding: 24rpx 52rpx;
}

.modal-title {
  padding: 20rpx 0;
  color: #000;
  font-size: 38rpx;
  text-align: center;
}

.btn-modal {
  height: 96rpx;
  line-height: 96rpx;
  color: #3cc51f;
  border-top: 1rpx solid #f4f4f5;
  text-align: center;
  display: block;
}

.modal-contents-plabel {
  width: 126rpx;
}

.content-tips {
  font-size: 28rpx;
  color: #999;
  margin-bottom: 15rpx;
}
