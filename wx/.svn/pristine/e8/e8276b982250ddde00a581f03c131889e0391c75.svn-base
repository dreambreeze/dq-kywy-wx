<template name="store-address">
  <view class="store-address">
    <view class="flex-wrp">
      <view class="aress-item">
        <view>{{!storename ? '未设置门店名称' : storename}}</view>
        <view class="text-ellipsis">{{!storeaddress ? '未设置地址' : storeaddress}}</view>
      </view>
      <view class="aress-item" data-address="{{storeaddress}}" data-sname="{{storename}}" bindtap="location"></view>
      <view class="aress-item" data-tel="{{tel}}" bindtap="callphone"></view>
    </view>
  </view>
</template>