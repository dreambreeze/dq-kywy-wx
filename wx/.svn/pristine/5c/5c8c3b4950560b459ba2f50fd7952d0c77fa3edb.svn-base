.add-info{
  padding: 0 160rpx;
  width: 430rpx;
  height: 254rpx;
}
.add-info .a-contact,.add-info .a-phone{
  display: flex;
  height: 34rpx;
  align-items: center;
  padding: 44rpx 0 22rpx 0;
  border-bottom:1rpx solid rgba(0,0,0,0.1)
}
.add-info .desc{
  flex:1;
  min-width: 120rpx;
}
.add-info .iview{
  width: 100%
}
.add-info  input{
  color: #333;
  padding-left: 56rpx;
  font-size: 28rpx;
}

.add-prc{
  margin: 20rpx 0 160rpx 0
}
.add-prc .prc-out{
  height: 90rpx;
  margin: 0 36rpx 32rpx;
  display: flex;
  align-items: center;
  border: 1rpx solid rgba(0,0,0,0.1);
  padding: 0 20rpx
}
.prc-out .section{
  text-align: right;
  flex: 1;
  color: #999;
  font-size: 26rpx;
}
.prc-out .prc-desc{min-width: 150rpx}
.prc-out .section.choosed{color: #333}
.prc-out .section .weui-input{
white-space:nowrap;
text-overflow:ellipsis;
overflow:hidden;
width:448rpx;
}
.prc-out .prc-down image{
  width: 19rpx;
  height: 11rpx;
  margin-left: 12rpx;
}
.input{
  z-index: 0;
  position: relative;
  margin: 12rpx 36rpx ;
}
.input textarea{
  border-radius: 4rpx;
  background-color: #eee;
  height: 158rpx;
  padding: 18rpx 26rpx;
  width: auto;
  color: #333;
  font-size: 28rpx;
  line-height: 36rpx;
}
.input .p-class{
  color: #999;
  font-size: 28rpx;
}

.submitadd{
  margin: 0 157rpx;
  width: 436rpx;
  background-color: #f1bbba;
  height: 88rpx;
  border-radius: 4rpx;
  color: #fff;
  font-size: 34rpx;
  font-weight: bold;
  text-align: center;
  line-height: 88rpx;
}
.submitadd.complete{
  background-image: linear-gradient(to bottom right , #e23f19,#d52f2e);
  box-shadow: 0  10rpx 50rpx rgba(255,0,0,0.1)
}