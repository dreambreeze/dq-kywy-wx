          <image src="https://iservice.daqisoft.cn/Uploads/img/2017-08-15/59925c54d0bfd150276411697520.jpg"></image>
        </view>
        <view class="pject-info-box clear">

          <view class="pject-info">
            <view class="pject-name">
              <text>中医足浴</text>90分钟</view>
            <view class="clear">
              <view class="discount">
                <text>￥</text>
                <text class="">239.00</text>
              </view>
              <view class="our-price">
                <text>￥</text>
                <text>239.00</text>
              </view>
            </view>
          </view>

          <view class="pject-on">×3</view>

          <view class="edit-box" hidden>
            <view class="flex-wrp">
              <view class="edit-fun">
                <text class="funs" bindtap="lessNum">－</text>
                <text class="nums">{{nums}}</text>
                <text class="funs" bindtap="addNum">＋</text>
              </view>
              <view class="edit-btn">删除</view>
