<import src="../../../common/buy-nav.wxml" />
<view class="container">
  <view class="car-item">
    <view class="car-title">
      <text class="all-check"></text>
      <navigator url="../project-store/project-store" hover-class="none" open-type="redirect">
        <text class="store-name">和毅体育新城店</text>
        <text class="store-arrow"></text>
      </navigator>
      <text class="car-edit">编辑</text>
    </view>

