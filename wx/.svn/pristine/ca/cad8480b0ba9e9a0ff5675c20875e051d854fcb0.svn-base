<import src="../common/engin-nav.wxml" />
<view class="contianer" style="min-height:{{minHeight}}">
  <view class="head-img-box">
    <block wx:if="{{userInfo.avatarurl}}">
      <view class="head-img">
        <image src="{{userInfo.avatarurl}}" lazy-load bindtap="infobind"></image>
        <view class="nickname">{{userInfo.nickname}}</view>
      </view>
    </block>
    <block wx:else>
      <view class="head-img">
        <button open-type="getUserInfo" bindgetuserinfo="getUInfo" class="getUInfo" hover-class="none"></button>
        <button open-type="getUserInfo" bindgetuserinfo="getUInfo" class="nickname" hover-class="none">点击登录</button>
      </view>
    </block>
  </view>

  <view class="center-nav active">
    <view class="flex-wrp">

      <view class="cnav-item" bindtap="ucenterNav" data-isto="{{true}}" data-url="../ucentermodel/pages/orders/orders">
        <image src="https://iservice.daqisoft.cn/Public/Home/images/my_order_icon.png" lazy-load></image>
        <view>我的订单</view>
      </view>

      <view class="cnav-item" bindtap="ucenterNav" data-isto="{{false}}" data-url="/pages/component/pages/coupons-pack/coupons-pack">
        <image src="https://iservice.daqisoft.cn/Public/Home/images/my_coupon_icon_notclick.png" lazy-load></image>
        <view style="color:#909090">我的优惠券</view>
      </view>

      <view class="cnav-item" bindtap="ucenterNav" data-isto="{{true}}">
        <image src="https://iservice.daqisoft.cn/Public/Home/images/history_icon.png" lazy-load></image>
        <view style="color:#909090">历史消费</view>
      </view>

    </view>
  </view>

  <view class="center-nav">
    <view class="flex-wrp">
      <view class="cnav-item" bindtap="ucenterNav" data-isTo="{{true}}" data-url="../ucentermodel/pages/infobind/infobind">
        <image src="https://iservice.daqisoft.cn/Public/Home/images/yue_project.png" lazy-load></image>
        <view>绑定手机</view>
      </view>
    </view>
  </view>

</view>

<view class="jishu-zhichi" bindtap="jishuzhichi">技术支持：大旗软件</view>
<!-- 分享按钮 -->
<view class="share-box">
  <button hover-class="none" open-type="share">
    <text>分享</text>
  </button>
</view>
<!-- 导航公用模版 -->
<template is="engin-nav" data="{{type:3,navArr: navTabBar,showImgUrl: showImgUrl}}" />