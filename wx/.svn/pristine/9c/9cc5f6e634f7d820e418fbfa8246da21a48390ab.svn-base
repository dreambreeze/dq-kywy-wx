<import src="../../../common/reserve-stime.wxml" />
<view class="container">
  <view class="top-store">
    <view class="flex-wrp">
      <image class="store-img" src="https://iservice.daqisoft.cn/Public/Home/images/home_banner.png" lazy-load></image>
      <view class="store-name">
        <view class="s-name">和毅足道滨江店</view>
        <view class="s-coupon">
          <image src="https://iservice.daqisoft.cn/Public/Home/images/bulletin_icon.png" lazy-load></image>
          您有一张优惠券待领取</view>
      </view>
      <view class="change-store">
        <view class="change-store-btn btn-gradient" bindtap="changeStoreBtn">切换门店
          <image src="https://iservice.daqisoft.cn/Public/Home/images/double_arrow_icon.png" lazy-load></image>
        </view>
      </view>
    </view>
  </view>

  <view class="top-store-address">
    <view class="flex-wrp">
      <view class="address text-ellipsis">
        <image src="https://iservice.daqisoft.cn/Public/Home/images/location_icon_1.png" lazy-load></image>
        长沙市雨花区人民东路与万家丽
      </view>
      <view class="location">699.3km
        <image src="https://iservice.daqisoft.cn/Public/Home/images/location_icon_2.png" lazy-load></image>
      </view>
      <view class="telphone">
        <image src="https://iservice.daqisoft.cn/Public/Home/images/telphone_icon_1.png" lazy-load></image>
      </view>
    </view>
  </view>

  <view class="room-people">
    <view class="flex-wrp">
      <view class="room-num room-people-num">
        <view class="room-box clear room-people-touch" bindtouchstart="roomPStart" bindtouchmove="roomPMove" bindtouchend="roomPEnd" style="left:{{roomPLeft}}">
          <view class="active">全部
            <text></text>
          </view>
          <view>双人间
            <text></text>
          </view>
          <view>三人间
            <text></text>
          </view>
          <view>四人间
            <text></text>
          </view>
          <view>五人间
            <text></text>
          </view>
          <view>六人间
            <text></text>
          </view>
          <view>七人间
            <text></text>
          </view>
          <view>八人间
            <text></text>
          </view>
        </view>
      </view>
      <view class="room-num-more {{pMactive}}" bindtap="roomPMore"></view>
    </view>

    <view class="room-people-more" style="display:{{pmoff ? 'none' : 'block' }}">
      <scroll-view scroll-y style="height: 300rpx">
        <view class="room-people-more-con clear">
          <view>六人间</view>
          <view>七人间</view>
          <view>八人间</view>
          <view>八人间</view>
          <view>八人间</view>
          <view>八人间</view>
          <view>六人间</view>
          <view>七人间</view>
          <view>八人间</view>
          <view>八人间</view>
          <view>八人间</view>
          <view>八人间</view>
          <view>六人间</view>
          <view>七人间</view>
          <view>八人间</view>
          <view>八人间</view>
          <view>八人间</view>
          <view>八人间</view>
        </view>
      </scroll-view>
    </view>
  </view>

  <view class="room-type">
    <view class="flex-wrp">
      <view class="room-num room-type-num">
        <view class="room-box clear room-type-touch" bindtouchstart="roomTStart" bindtouchmove="roomTMove" bindtouchend="roomTEnd" style="left:{{roomTLeft}}">
          <view class="active">全部
            <text></text>
          </view>
          <view>棋牌房
            <text></text>
          </view>
          <view>SPA房
            <text></text>
          </view>
          <view>普足房
            <text></text>
          </view>
          <view>中医房
            <text></text>
          </view>
          <view>中医房
            <text></text>
          </view>
          <view>中医房
            <text></text>
          </view>
        </view>
      </view>
      <view class="room-num-more {{tMactive}}" bindtap="roomTMore"></view>
    </view>

    <view class="room-type-more" style="display:{{tmoff ? 'none' : 'block' }}">
      <scroll-view scroll-y style="height: 300rpx">
        <view class="room-people-more-con clear">
          <view>棋牌房</view>
          <view>棋牌房</view>
          <view>棋牌房</view>
          <view>棋牌房</view>
          <view>棋牌房</view>
          <view>棋牌房</view>
          <view>棋牌房</view>
          <view>棋牌房</view>
          <view>棋牌房</view>
          <view>棋牌房</view>
          <view>棋牌房</view>
          <view>棋牌房</view>
          <view>棋牌房</view>
          <view>棋牌房</view>
          <view>棋牌房</view>
          <view>棋牌房</view>
          <view>棋牌房</view>
          <view>棋牌房</view>
        </view>
      </scroll-view>
    </view>
  </view>

  <!-- 房间 -->
  <view class="room-content clear">

    <view class="room-item" wx:for="{{roomArr}}">
      <view class="room-img" bindtap="toRoomDil">
        <image src="https://iservice.daqisoft.cn/Public/Home/images/home_banner.png" lazy-load></image>
        <view class="room-info"><text>【SPA房】单人间</text></view>
      </view>
      <view class="reserve-room-btn btn-gradient" bindtap="reserveBtn">预约</view>
    </view>

  </view>

</view>

<!-- 分享按钮 -->
<view class="share-box">
  <button hover-class="none" open-type="share">
    <text>分享</text>
  </button>
</view>

<!-- 选择时间模版 -->
<template is="reserve-stime" data="{{maskDisplay: maskDisplay,showSelectTime: showSelectTime}}" />