@import "../../../common/online-reserve.wxss";
@import "../../../common/reserve-stime.wxss";
.container{
  padding-bottom: 150rpx;
}

.head{
  display: flex;
  display: -webkit-flex;
  align-items: center;
  border-bottom: 1rpx solid rgba(0,0,0,0.1);
  padding: 20rpx 0
}
.head .shopimg image{
width: 120rpx;
height: 120rpx;
margin:0 28rpx;
}

.head .shopname view:first-child{
font-weight:bold;
font-size:34rpx;
margin: -22rpx 0 20rpx 0 ;
}
.head .shopname view:last-child{
font-size:24rpx;
background-color:rgba(255,0,0,0.1);
height:58rpx;
line-height:58rpx;
padding:0 20rpx;
border:1rpx solid rgba(255,0,0,0.2);
color: rgba(255,0,0,0.6);
}
.head .changeshop{
height: 68rpx;
width: 200rpx;
font-size:28rpx;
font-weight:bold;
color: #fff;
display: flex;
display: -webkit-flex;
justify-content: flex-end;
align-items: center;
background-image: linear-gradient(to right bottom, #dfceb5,#c59f86);
background-image: -webkit-gradient(to right bottom ,#dfceb5,#c59f86);
position:absolute;
right:0;
border-bottom-left-radius:42rpx;
border-top-left-radius:42rpx;
}
.head .changeshop image{
  width: 20rpx;
  height: 20rpx;
  margin: 0 24rpx 0 8rpx;
}

.shopinfo {
font-size: 24rpx;
color: #666;
width: 100%;
height: 80rpx;
display: flex;
align-items: center;
}
.shopinfo .s-addr{
width: 65%;
height: 80rpx;
white-space: nowrap;
text-overflow: ellipsis;
overflow: hidden;

display: flex;
align-items: center;
}
.shopinfo .s-addr image{
  width: 19rpx;
  height: 25rpx;
  margin:  0  10rpx 0 28rpx;
}
.shopinfo .s-distance{
  border-left: 1rpx solid rgba(0,0,0,0.1);
  display: flex;
  display: -webkit-flex;
  align-items: center;
  width: 25%;
  justify-content: center;
}
.s-distance image{
  width: 38rpx;
  height: 35rpx;
  margin: 0 4rpx;
}
.shopinfo .s-phone{
  border-left: 1rpx solid rgba(0,0,0,0.1);
  width: 10%;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
}
.s-phone image{
   width: 33rpx;
  height: 36rpx;
}

.t-line{
  width:100%;
  height: 20rpx;
  background-color: #eee;
}

.content {
  height: 100%;
}
.content .c-search{
border-bottom:1rpx solid rgba(0,0,0,0.1);
margin:20rpx 160rpx;
}
.c-search .plhc{
  font-size: 24rpx;
  text-align: center;
}
.c-search input{
  font-size: 24rpx;
  padding: 5rpx;
}

.c-class{
  display: flex;
  display: -webkit-box;
  align-items: center;
  justify-content: center;
  margin: 6rpx 40rpx;
} 
.c-class .c-item{
text-align: center;
line-height: 64rpx;
max-width:260rpx;
padding:0 15rpx;
font-size: 28rpx;
font-weight: bold;
height: 56rpx;
}
.c-class .c-item:last-child{
  width: 20%;
}
.c-class .c-item.active{
  color: #caa876;
}
.c-class .active::after{
content: "";
border-radius: 4rpx;
display: block;
height:8rpx;
width:40%;
background-color:#caa876;
transition:width 0.3s ease 0s;
margin: 0 auto;
}

.t-info{
width: 100%;
height: auto;
padding: 30rpx 2rpx;
display:flex;
flex-wrap:wrap;
justify-content:space-around;
}
.t-info .t-item{
  width: 344rpx;
  height: 520rpx;
  border: 1rpx solid rgba(0,0,0,0.1);
  margin: 10rpx 6rpx;
  border-top-right-radius: 10rpx;
  border-top-left-radius: 10rpx;
  position: relative;
}
.t-item .i-img image{
  width: 346rpx;
  height: 343rpx;
}
.t-item .i-no{
  font-weight:bold;
  font-size:28rpx;
  padding: 0rpx 8rpx;
}
.t-item .i-detail{
  color: #caa876;
  font-size: 24rpx;
  padding: 5rpx 10rpx;
}
.t-item .i-today{
  height: 48rpx;
  width: 112rpx;
  position: absolute;
  top: 0;
  font-size: 26rpx;
  color: #fff;
  background-image: linear-gradient(to top left ,#dfceb5 , #c59f68);
  background-image:-webkit-linear-gradient(to top left ,#dfceb5 , #c59f68);
  text-align: center;
  line-height: 48rpx;
  border-radius: 10rpx;
}


.reserve-room-p{
  text-align: center;
  padding-top: 20rpx;
  min-height: 60rpx;
}

.reserve-room-p icon{
  display: none;
}

.reserve-room-btn{
  display: inline-block;
  width: 150rpx;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 10rpx;
  text-align: center;
  font-size: 28rpx;
  color: #fff;
  box-shadow: 0 4rpx 12rpx rgba(199, 156, 98, 0.5);
}

.buy-bar {
  width: 100%;
  height: 97rpx;
  line-height: 97rpx;
  background: #be9e70;
  position: fixed;
  bottom: 0;
  left: 0;
  text-align: center;
  font-size: 34rpx;
  color: #fff;
  font-weight: 500;
}

.nomore{
display:flex;
justify-content:center;
align-items:center;
font-size:22rpx;
color:#666;
}
.nomore::after{
  content: "--"
}
.nomore::before{
  content: "--"
}