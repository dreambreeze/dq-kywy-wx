.project-info {
  padding: 24rpx 30rpx;
  border-bottom: 30rpx solid #eee;
}

.p-img {
  width: 205rpx;
  height: 160rpx;
  margin-right: 32rpx;
}

.p-info {
  width: 334rpx;
  line-height: 1;
}

.p-name {
  font-size: 34rpx;
  color: #333;
  padding: 18rpx 0 26rpx;
}

.p-time {
  margin-bottom: 16rpx;
}

.p-time, .p-store {
  font-size: 24rpx;
  color: #666;
}

.p-time image {
  width: 24rpx;
  height: 24rpx;
  margin-right: 14rpx;
}

.p-store image {
  width: 23rpx;
  height: 21rpx;
  margin-right: 14rpx;
  vertical-align: middle;
  margin-top: -2rpx;
}

.p-price {
  flex: 1;
}

.p-price view:nth-of-type(1) {
  font-size: 30rpx;
  color: #e30101;
  margin-top: 60rpx;
}

.p-price view:nth-of-type(2) {
  font-size: 26rpx;
  color: #c5c5c5;
  text-decoration: line-through;
}

.reserve-num {
  padding: 0 30rpx;
  background: #fff;
  height: 94rpx;
  line-height: 94rpx;
  color: #333;
  font-size: 28rpx;
  border-bottom: 9rpx solid #eee;
}

.num-fun {
  float: right;
}

.num-fun text {
  display: inline-block;
}

.num-fun text:nth-of-type(1), .num-fun text:nth-of-type(3) {
  width: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
  font-size: 30rpx;
  border: 1rpx solid #caa876;
  border-radius: 6rpx;
  color: #caa876;
  text-align: center;
}

.num-fun text:nth-of-type(2) {
  font-size: 28rpx;
  min-width: 62rpx;
  padding: 0 12rpx;
  text-align: center;
}

.selecter-iner {
  padding: 0 30rpx;
  background: #fff;
  height: 90rpx;
  line-height: 90rpx;
  color: #333;
  font-size: 28rpx;
  border-bottom: 22rpx solid #eee;
}

.select-tips {
  float: right;
  font-size: 30rpx;
  color: #be9e70;
}

.select-tips image {
  margin-left: 36rpx;
}

.select-tips image, .coupon-tips image {
  width: 10rpx;
  height: 18rpx;
  vertical-align: middle;
}

.coupon-tips {
  float: right;
}

.coupon-tips text {
  display: inline-block;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 22rpx;
  background: #ec7206;
  color: #fff;
  padding: 0 4rpx;
  border-radius: 6rpx;
}

.coupon-tips image {
  margin-left: 22rpx;
}

.input-box {
  padding: 30rpx;
  background: #fff;
  position: relative;
}

.input-box textarea {
  width: 100%;
  border: 1rpx solid #ccc;
  border-radius: 6rpx;
  height: 134rpx;
  font-size: 24rpx;
}

.input-limit {
  position: absolute;
  right: 44rpx;
  bottom: 44rpx;
  font-size: 24rpx;
  color: #999;
}

.order-bar{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  border-top: 1rpx solid #d6d6d6;
  box-sizing: border-box;
  height: 100rpx;
  line-height: 100rpx;
}

.order-price{
  width: 436rpx;
}

.order-btn{
  flex: 1;
  background: #353535;
  font-size: 34rpx;
  font-weight: bold;
  color: #be9e70;
  text-align: center;
}

.p-num{
  font-size: 28rpx;
  color: #666;
  margin-left: 44rpx;
}

.price{
  float: right;
  margin-right: 40rpx;
  font-size: 30rpx;
  color: #e02626;
}

.price text:nth-of-type(1){
  color: #333333
}