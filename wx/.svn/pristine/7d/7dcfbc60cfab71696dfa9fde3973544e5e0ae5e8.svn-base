@import "../common/engin-nav.wxss";

.container {
  background: #eee;
  min-height: 1182rpx;
}

.banner image {
  width: 100%;
  height: 342rpx;
}

.eBuy-nav {
  font-size: 36rpx;
  width: 600rpx;
  height: 116rpx;
  line-height: 116rpx;
  text-align: center;
  font-weight: bold;
  color: #cfb287;
  background: #333;
  border-radius: 30rpx;
  margin: 18rpx auto 54rpx;
  box-shadow: 2rpx 10rpx 12rpx rgba(207, 178, 135, 0.3);
}

.to-website-icon {
  display: inline-block;
  width: 22rpx;
  height: 22rpx;
  background: url(https://iservice.daqisoft.cn/Public/Home/images/to_website_icon.png) no-repeat;
  background-size: contain;
  vertical-align: middle;
  margin-left: 26rpx;
}

.pro-nav {
  padding: 28rpx 0;
  background: #fff;
  box-sizing: border-box;
}

.nav-item {
  height: 94rpx;
  line-height: 94rpx;
  width: 50%;
  text-align: center;
  font-size: 28rpx;
  color: #333;
}

.nav-item view {
  line-height: 1;
  display: inline-block;
  margin-left: 28rpx;
}

.nav-item image {
  width: 42rpx;
  vertical-align: middle;
}

.nav-item:nth-of-type(1) {
  border-right: 1rpx solid #e8e8e8;
}

.nav-item:nth-of-type(1) image {
  height: 48rpx;
}

.nav-item:nth-of-type(2) image {
  height: 42rpx;
  margin-top: -4rpx;
}

.nav-item button {
  font-size: 28rpx;
  color: #333;
  line-height: 94rpx;
  border: none;
  box-sizing: initial;
  background-color: #fff;
  padding: 0;
}

.nav-item button::after {
  content: none;
}

.jishu-zhichi {
  position: fixed;
  bottom: 110rpx;
  left: 0;
  width: 100%;
}
