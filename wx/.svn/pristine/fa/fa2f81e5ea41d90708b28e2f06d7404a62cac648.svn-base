 @import "../../wxParse/wxParse.wxss"; 

page {
  height: 100%;
  overflow: hidden;
} 
.container{
  height: 100%;
}
.container view{
  overflow: unset;
}
.container .wxParse view{
  overflow: auto
}
.container scroll-view{
  height: 100%;
  width: 100%;
}
.banner{
  height: 512rpx;
}
.swiper {
  height: 512rpx;
}
.swiper swiper-item .slide-image{
  width: 750rpx;
  height: 512rpx;
}

.goodsinfo{
  padding:20rpx 30rpx;
}
.goodsinfo view{
  padding: 8rpx 0;
}
.goodsinfo .goodsprice{
  color: #db0000;
  font-weight: bold;
}
.goodsinfo .goodsothers{
  padding: 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 24rpx;
  color: #666;
}
.goodsothers view:first-child{
  font-size: 20rpx;
  padding: 3rpx;
  color: #f47c08;
  border: 1rpx solid #f47c08;
  border-radius: 8rpx;
}

.evaluation{
  padding: 20rpx 30rpx;
}
.evaluation .e-head{
  margin-bottom: 20rpx;
}
.evaluation .e-body{
  margin-bottom: 30rpx;
}
.e-body .b-head{
  display: flex;
  align-items: center;
}
.b-head .userimg{
  width: 60rpx;
  height: 60rpx;
  margin: 0 12rpx;
}
.b-head .userstar{
  margin: 0 12rpx;
}
.b-head .userstar image{
  width: 31rpx;
  height: 29rpx;
  margin: 0 4rpx;
}
.e-body .b-content{
  line-height: 36rpx;
  padding: 14rpx 0;
}
.e-body .b-time{
  color: #666;
  font-size: 24rpx;
  display: flex;
  align-items: center;
}
.b-time .usertime{
  width: 300rpx;
}
.e-seeall{
  width: 256rpx;
  height: 52rpx;
  line-height: 52rpx;
  text-align: center;
  color: #db0000;
  border: 1rpx solid #db0000;
  border-radius: 48rpx;
  margin: 10rpx auto;
}

.goodsdesc {
  padding-bottom: 36rpx;
}
.goodsdesc image{
  width: 750rpx;
  height: 2508rpx
}

.by {
margin-bottom: 150rpx;
}
.by image{
   width: 687rpx;
   height: 822rpx;
}
.goodscart{
  position: fixed;
  bottom: 0;
  left: 0; 
  display: flex;
  background: #fff;
  width: 100%;
}
.goodscart .cuservice,.cart{
  width: 100rpx;
  height: 100rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  border-top: 1rpx solid rgba(0,0,0,0.2);
  font-size: 24rpx;
  color: #666;
}
.goodscart .cuservice button{
padding:0;
box-sizing:unset;
background-color:#fff;
line-height:normal;
border-radius:0px;
position: absolute;
height:100rpx;
width:100rpx;
font-size:26rpx;
display:flex;
flex-direction:column;
justify-content:center;
align-items:center;
color:#666;

}
.goodscart .cuservice image{
  width: 35rpx;
  height: 39rpx;
}
.goodscart .cart{
  width: 150rpx;
  border-left: 1rpx solid rgba(0,0,0,0.2);
  position: relative; 

}
.cart .count{
  position:absolute;
  left:82rpx;
  font-size:22rpx;
  width: 32rpx;
  height: 32rpx;
  color:#fff;
  background:#cf0100;
  border-radius:50%;
  top: 0rpx;
}
.goodscart .cart image{
  width: 35rpx;
  height: 39rpx;
}
.goodscart .addcart,.tobuy{
  width: 250rpx;
  height: 100rpx;
  color: #fff;
  text-align: center;
  line-height: 100rpx;
}
.goodscart .addcart{
  background-color: #ef7101;
}
.goodscart .tobuy{
  background-image:linear-gradient(to bottom right,#df2a00,#d21717);
}

/*选择商品规格  */
.goodsselect .mask{
  background-color: rgba(0,0,0,0.7);
  padding-bottom: 100rpx;
}
.goodsselect, .goodsselect .mask{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.goodsselect .cartdetail {
  position: absolute;
  left: 0;
  bottom: 0rpx;
  width: 100%;
  background: #fff;
  font-size: 28rpx;
}


.cartdetail .c-head{
display:flex;
/* align-items:center; */
padding:20rpx;
}
.c-head .gimg{
  border:6rpx solid #fff;
  margin-top:-40rpx;
  border-radius: 8rpx;

}
.c-head .gimg image{
  width: 192rpx;
  height: 192rpx;
  border-radius: 8rpx
}
.c-head .ginfo{
  flex: 1;
  margin-left: 30rpx;
}
.ginfo  .gprice{
  color: #db0000;
  font-size: 32rpx;
}
.c-head .ginfo view{
  margin-top: 10rpx;
}
.c-head .close image{
  width: 72rpx;
  height: 70rpx;
}

.cartdetail .g-size{
  padding: 10rpx 26rpx;
}
.g-size .s-title{
  margin-bottom: 10rpx;
}
.g-size .s-info{
  display: flex;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid rgba(0,0,0,0.2);
}
.g-size .s-info .active{
  background-color: #db0000;
  color: #fff;
}
.s-info view{
  display: flex;
  align-items: center;
  justify-content: center;
  min-width: 100rpx;
  padding: 0 10rpx;
  height: 60rpx;
  background-color: #eee;
  margin-right: 30rpx;
  border-radius: 8rpx;
}
.g-ordernum{
  display: flex;
  padding: 0rpx 26rpx 320rpx 26rpx;
  height: 100rpx;
}
.g-ordernum view{
  height: 100rpx;
}
.g-ordernum .o-title{
  flex: 1;
  line-height: 100rpx;
}

.g-ordernum .o-reduce,.o-add{
  width: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.g-ordernum .o-item{
width:50rpx;
height:50rpx;
background-color:#eee;
display:flex;
justify-content:center;
align-items:center;
font-size:32rpx;
border-radius:8rpx;
}
.g-ordernum .o-num{
  line-height: 104rpx;
  width: 64rpx;
  text-align: center;
}
.shopsubmit{
height: 100rpx;
background-color: #db0000;
display:flex;
align-items:center;
justify-content:center;
color:#fff;
font-size:34rpx;
}

/*修改购买数量*/
.updatenum .mask{
  background-color: rgba(0,0,0,0.7);
  padding-bottom: 100rpx;
}
.updatenum{
display:flex;
padding-top: 320rpx;
justify-content:center;
}

.updatenum, .updatenum .mask{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.updatenum .numdetail {
  position: absolute;
  width: 520rpx;
  background: #fff;
  font-size: 28rpx;
  border-radius: 10rpx;

}
.numdetail .n-title{
  padding: 20rpx 30rpx;
}
.updatenum .n-update{
  display: flex;
  justify-content: center;
  height: 100rpx;
  padding: 20rpx 30rpx;
}
.n-update .o-reduce,.o-add{
  width: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.n-update .o-item{
width:50rpx;
height:50rpx;
background-color:#eee;
display:flex;
justify-content:center;
align-items:center;
font-size:32rpx;
border-radius:8rpx;
}
.n-update .o-num{
  display:flex;
  align-items:center;
  width: 64rpx;
  text-align: center;
}
.operate{
display:flex;
height:72rpx;
}
.operate .cancel,.operate .ok{
width:50%;
display:flex;
align-items:center;
justify-content:center;
border-top: 1rpx solid rgba(0,0,0,0.1);
}
.operate .ok{
  background-color: #db0000;
  color: #fff;
  border-bottom-right-radius: 10rpx;
}