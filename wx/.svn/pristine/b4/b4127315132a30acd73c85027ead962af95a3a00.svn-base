Page({

  /**
   * 页面的初始数据
   */
  data: {
    
  },

  /**
   * 用户点击右上角分享
   */
  onShareAppMessage: function () {
    return {
      title: '客源无忧微信营销平台'
    }
  },

  /**
   * 页面跳转
   */
  jumpPage: function(ev){
    var url = ev.currentTarget.dataset.url;
    if (typeof url != 'undefined'){
      wx.navigateTo({
        url: url,
      });
    }
  }
})