.page-head{
  width: 100%;
  height: 140rpx;
}
.page-head .h-red{
  height: 70rpx;
  background-color: #cf0100
}
.page-head .h-white{
  height: 70rpx;
  background-color: #fff
}
.page-head .h-search{
  height: 80rpx;
  width: 590rpx;
  margin: -118rpx 80rpx;
  box-shadow: 0 12rpx 40rpx rgba(0,0,0,0.1);
  border-radius: 40rpx;
  background-color: #fff;
  justify-content: space-between
}
 .h-search .searchimg{
   width: 58rpx;
   justify-content: flex-end
 }
 .h-search image{
   width: 28rpx;
   height: 28rpx
 }
 .h-search input{
   width: 364rpx
 }
 .h-search .searchsubmit{
  height:80rpx;
  width:128rpx;
  background-color:#eee;
  border-top-right-radius: 40rpx;
  border-bottom-right-radius: 40rpx;
  font-weight: bold;
  color: #d21717;
 }

 .history{
  width: 100%;
  height: 100%;
}
.history .h-title{
display:flex;
align-items:center;
height: 72rpx;
padding: 0 40rpx;
}
.h-title .t-left{
display:flex;
align-items:center;
font-size: 28rpx;
color: #666;
flex: 1;
}

.t-left image{
  width: 28rpx;
  height: 28rpx;
  margin-right: 8rpx;
}
.t-right {
  width: 72rpx;
  height: 72rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.t-right image{
  width: 26rpx;
  height: 26rpx;
}

.historyItem{
display:flex;
flex-wrap:wrap;
justify-content: flex-start;
padding: 20rpx 40rpx;
font-size: 26rpx;
}
.history .item{
max-width:260rpx;
height:56rpx;
line-height:56rpx;
text-align:center;
background-color:#eee;
margin:0 30rpx 20rpx 0;
border-radius:8rpx;
white-space:nowrap;
text-overflow:ellipsis;
overflow:hidden;
padding:0 10rpx;
}
.history .hot image{
  width: 23rpx;height: 28rpx
}