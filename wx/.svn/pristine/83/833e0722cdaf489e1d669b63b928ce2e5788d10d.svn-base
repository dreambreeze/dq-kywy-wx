<import src="../common/engin-nav.wxml" />
<view class="contianer" style="min-height:{{minHeight}}">
  <view class="head-img-box">
    <view class="head-img">
      <image src="{{wXUserInfo.avatarUrl}}" lazy-load bindtap="infobind"></image>
      <block wx:if="{{wXUserInfo.nickName}}">
      <view class="nickname">{{wXUserInfo.nickName}}</view>
      </block>
      <block wx:else>
      <view class="nickname">已拒绝获取基本信息，前往<view class="go-settimg" bindtap="goSettimg">设置</view></view>
      </block>
    </view>
  </view>

  <view class="center-nav active">
    <view class="flex-wrp">
      <view class="cnav-item" bindtap="ucenterNav" data-url="../ucentermodel/pages/orders/orders">
        <image src="https://iservice.daqisoft.cn/Public/Home/images/my_order_icon.png" lazy-load></image>
        <view>我的订单</view>
      </view>
      <view class="cnav-item" bindtap="ucenterNav" data-url="/pages/component/pages/coupons-pack/coupons-pack">
        <image src="https://iservice.daqisoft.cn/Public/Home/images/my_coupon_icon.png" lazy-load></image>
        <view>我的优惠券</view>
      </view>
      <view class="cnav-item">
        <image src="https://iservice.daqisoft.cn/Public/Home/images/history_icon.png" lazy-load></image>
        <view>历史消费</view>
      </view>
    </view>
  </view>

  <view class="center-nav">
    <view class="flex-wrp">
      <view class="cnav-item" bindtap="ucenterNav" data-url="../ucentermodel/pages/infobind/infobind">
        <image src="https://iservice.daqisoft.cn/Public/Home/images/yue_project.png" lazy-load></image>
        <view>绑定手机</view>
      </view>
    </view>
  </view>

</view>

<view class="jishu-zhichi" bindtap="jishuzhichi">技术支持：大旗软件</view>
<!-- 分享按钮 -->
  <view class="share-box">
    <button hover-class="none" open-type="share"><text>分享</text></button>
  </view>
<!-- 导航公用模版 -->
<template is="engin-nav" data="{{type:3,navArr: navArr,showImgUrl: showImgUrl}}" />
