/*购物车  */
.cart-detail, .cart-detail .mask {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
}
.cart-detail .mask {
	background: rgba(0,0,0,0.7);
}
.cart-detail .list {
	position: absolute;
	left: 0;
	bottom: 100rpx;
	width: 100%;
	background: #f7f7f7;
	padding: 0rpx 0 30rpx 0;
}
.cart-detail .list .item {
	display: -webkit-flex;
  align-items:center;
	color: #333;
	font-size: 36rpx;
	line-height: 50rpx;
  height: 98rpx;
	padding: 0rpx 36rpx;
  animation-duration: 0.3s;
  -webkit-animation-duration: 0.3s;
  border-bottom: 1rpx rgba(0,0,0,0.1) solid;
}
.cart-detail .list .item .name {
	-webkit-flex: 1;
  font-size: 32rpx;
}
.cart-detail .list .item .total {
	width: 120rpx;
  font-size: 32rpx;
}
.cart-detail .list .item .reduce,
.cart-detail .list .item .add {
	font-size: 50rpx;
	/* background:  #1E90FF; */
	width: 57rpx;
	height: 57rpx;
	text-align: center;
  color: #fff;
	border-radius: 50%; 
}
.cart-detail .list .item .addimg{
  width: 57rpx;
	height: 57rpx;
}
.cart-detail .list .item .num {
	width: 50rpx;
	text-align: center;
  font-size: 28rpx;
	margin: 0 5rpx;
}

.cart-detail .clear {
  height: 88rpx;
  display: flex;
  display: -webkit-flex;
   align-items: center;
  
   padding: 0rpx 36rpx;
   color: #666;
   font-size: 24rpx;
   background-color: #eee;
  
} 
.clear  .clearimg{
  width: 28rpx;
  height: 28rpx;
  margin-right: 6rpx;
}
.clear .desc{
 -webkit-flex: 1;
 flex: 1;
}

.cart {
	display: -webkit-flex;
	position: absolute;
	left: 0;
	bottom: 0;
	width: 100%;
	height: 100rpx;
	background: #fff;
}
.cart .data {
	-webkit-flex: 1;

}
.cart .data .icon {
	position: absolute;
	left: 40rpx;
	top: -20rpx;
	width: 100rpx;
	height: 100rpx;
	/* background:  #1E90FF; */
	border-radius: 50%;
}
.cart .data .icon image {
	position: absolute;
	left:-4rpx;
  top:-2rpx;
  width:110rpx;
  height:110rpx;
}
.cart .data .icon .count {
	position: absolute;
	left: 70rpx;
	top: -10rpx;
	font-size: 30rpx;
	line-height: 40rpx;
	padding: 0 12rpx;
	color: #fff;
	background: #f45044;
	border-radius: 20rpx;
}
.cart .data .total {
	color: #f45044;
	font-size: 34rpx;
	line-height: 100rpx;
	padding-left: 160rpx;
  font-weight: bold;
} 
.cart button {
  border-radius:0px; 
  border: none;
	width: 240rpx;
	height: 100%;
  line-height: 100rpx;
	font-size: 34rpx;
}

.modal{
  background-color: red;
}

button.blue {
	color: #caa876;
	background: #353535;
}
button.blue:after {
	border-color: rgba(255, 133, 111, 0.3);
}
button.blue.hover {
	background: #353535;
}
button.blue.disabled {
  color: #fff;
  font-size: 34rpx;
	background:#b4b4b4;
}





.rules {
  position: fixed;
  top: 50%;
  left: 50%;
  z-index: 99999;
  width: 600rpx;
  height: 900rpx;
  margin-top: -450rpx;
  margin-left: -300rpx;
  background: #fff;
  border-radius: 3px;
  box-shadow: 0 0 25px rgba(0, 0, 0, 0.2);
  animation-name: notice;
  -webkit-animation-name: notice;
  animation-duration: 0.3s;
  -webkit-animation-duration: 0.3s;
  animation-fill-mode: both;
  -webkit-animation-fill-mode: both;
}

@keyframes notice {
  0% {
    opacity: 0.3;
    -ms-transform: scale(0.5);
    transform: scale(0.5);
  }

  100% {
    opacity: 1;
    -ms-transform: scale(1);
    transform: scale(1);
  }
}

@-webkit-keyframes notice {
  0% {
    opacity: 0.3;
    -webkit-transform: scale(0.5);
  }

  100% {
    opacity: 1;
    -webkit-transform: scale(1);
  }
}

