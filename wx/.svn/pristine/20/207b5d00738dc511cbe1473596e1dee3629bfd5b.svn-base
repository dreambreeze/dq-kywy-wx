        </block>
      </view>
      <view class='h-content'>
        <view class='c-head'>{{item.staffworkno}}号</view>
      </view>
    </view> -->
  </view>

  <!--预约房间  -->
  
  <view class='yyinfo'  bindtap='goyy' data-type="2"  >
    <view class='yytitle yytech'>
      <view class='ltitle'>预约房间</view>
      <view class='rtitle'>
        <block wx:if="{{!roomData}}">
          <view class='rword' >
          去预约房间
          <image src='https://iservice.daqisoft.cn/Public/Home/images/amimgs/toright.png'></image>
          </view>
        </block>
         <block wx:else>
          <view class='technos' wx:for="{{roomData}}" wx:key=""  wx:if="{{item.selected}}">[{{item.roomscategoryname}}] {{item.bednum?item.bednum+"人间":""}}</view>
        </block>
      </view>
      
    </view>
  </view>
