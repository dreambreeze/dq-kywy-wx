<view class="project-info">
  <view class="flex-wrp">
    <image class="p-img" src="{{currentReserveStore[0].store_img ? currentReserveStore[0].store_img : bannerImg}}" lazy-load></image>
    <view class="p-info">
      <!-- <view class="p-name">棋牌房</view> -->
      <view class="p-store">
        <image src="https://iservice.daqisoft.cn/Public/Home/images/order_store_icon.png" lazy-load></image>{{currentReserveStore[0].store_name}}</view>
       <!-- <view class="p-time">
        <image src="https://iservice.daqisoft.cn/Public/Home/images/order_time_icon.png" lazy-load></image>
        <text space="nbsp" decode>预约今天01-25 17:00</text>
      </view>  -->
      <view class="p-time">

        <text space="nbsp" decode>{{currentReserveStore[0].address}}</text>
      </view>
    </view>
  </view>
</view>

 <view class='yyinfo yynp'>
    <view class='yytitle'>预约信息</view>
    <view class='yytitle'>
      <view class='yyleft'>
        姓名：
      </view>
      <view class='yyright'>
        <input type="text" value="{{names}}" bindinput="namesVal" />
      </view>
    </view>
    <view class='yytitle'>
      <view class='yyleft'>
        电话：
      </view>
      <view class='yyright'>
        <input type="number" value="{{phone}}" bindinput="phoneVal" maxlength='11'/>
        <view class="getPhoneNumber">
            <button open-type="getPhoneNumber" bindgetphonenumber="getPhoneNumber">自动输入             </button>
      </view>
      </view>
    </view>
  </view>

  <view class='yyinfo' bindtap='goyy' data-type="1">
    <view class='yytitle yytech'>
      <view class='ltitle' >预约技师</view>
      <view class='rtitle'>
        <block wx:if="{{!tech}}">
          <view class='rword' >
          去预约技师
          <image src='https://iservice.daqisoft.cn/Public/Home/images/amimgs/toright.png'></image>
          </view>
        </block>
         <block wx:else>
          <view class='technos'>{{tech}}</view>
        </block>
      </view>
      
    </view>
    <!-- <view class='item ' wx:for="{{techdata}}" wx:key="" >
