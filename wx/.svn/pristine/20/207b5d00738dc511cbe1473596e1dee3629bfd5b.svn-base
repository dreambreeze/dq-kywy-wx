<view class="selecter-iner" > 保留时间：
  <view class="select-tips {{SelectedTime?'select-active':''}}">{{SelectedTime?SelectedTime+"分钟":"请选择到店时间"}}</view>
</view>

<view class="input-box" hidden='{{showTime}}'>
  <textarea maxlength="50" placeholder="备注：可将您其它要求告知我们" cursor-spacing="50" bindinput="remarks"></textarea>
  <view class="input-limit">{{inputLimit}}</view>
</view>

<view class="order-bar" bindtap="toSuccess">确定下单</view>


 <!--点击预约 选择时间  -->
   <view class='tp-time' hidden='{{!showTime}}'> 
  <!-- <view class='tp-time' > -->
    <view class='mask' bindtap='hideTPTime'></view>
    <view class='timeinfo'>
      <view class='ti-head'>
        <view class='ti-hline'></view>
        <view class=''>选择到店时间</view>
        <view class='ti-hline'></view>
        <image src='https://iservice.daqisoft.cn/Public/Home/images/techimgs/cancel.png' bindtap='hideTPTime'></image>
      </view>

      <view class='selecttime'>
        <block wx:for="{{dateArray}}" wx:key="" >
          <view class="slt-item {{item==SelectedTime?'active':''}}" bindtap='toSelectTime' data-SelectedTime='{{item}}'>
           {{item}}
          </view>
        </block>
      </view>


      <view class='timesubmit' bindtap='hideTPTime'>
        <view>确定</view>
      </view>

    </view>
  </view>