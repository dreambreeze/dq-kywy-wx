Page({

  /**
   * 页面的初始数据
   */
  data: {
    maskDisplay: 'none'
  },

  /**
   * 生命周期函数--监听页面加载
   */
  onLoad: function (options) {

  },
  /**
   * 跳转至项目详情页
   */
  navigatordetail: function () {
    wx.navigateTo({
      url: '../project-detail/project-detail',
    })
  },
  /**
   * 项目分类动画开始
   */
  categorystart: function(e){
    console.log(e);
  },
  /**
   * 点击跳转项目搜索页
   */
  toSearchProject: function(){
    wx.navigateTo({
      url: '../search-project/search-project',
    });
  },
  /**
   * 点击预约显示选择时间
   */
  reserveBtn: function(){
    this.setData({
      showSelectTime: 'showIn',
      maskDisplay: 'block'
    });
  },
  /**
   * 点击关闭选择时间
   */
  closeStime: function(){
    this.setData({
      maskDisplay: 'none',
      showSelectTime: 'showOut'
    });
  },
  /**
   * 点击跳转至选择门店
   */
  changeStoreBtn: function(){
    wx.navigateTo({
      url: '../select-store/select-store',
    });
  },
  /**
   * 跳转确认下单页
   */
  confirmBtn: function(){
    wx.navigateTo({
      url: '../order-confirm/order-confirm',
    });
  }
})