<view class='amcontainer'>
<block wx:if="{{ordesData[groupno].buytype==1}}">
  <view class='head'>
      <view class='h-img'>
        <image  src='{{compeleteimg}}'></image>
      </view>
      <view class='h-desc'>支付成功!</view>
       <!-- <view class='h-time'>预计<text >{{arrivetime}}分钟</text>送达</view> -->
  </view>
  
  <view class="nav">
      <view bindtap='toorder'>查看订单</view>
      <navigator url='../../../index/index' open-type="redirect" hover-class="none">
        <view>返回首页</view>
      </navigator>
  </view>
</block>
<!-- 参团开团团 -->
<block wx:else>
     <view class='group-head' bindtap='gobuy' data-id='{{project.id}}' data-groupno='{{groupno}}'>
    <view class='item flex'>
      <view class='h-img'>
        <image  mode='aspectFill' src='{{project.picurl[0]}}'></image>
      </view>
      <view class='h-content'>
        <view class='c-head'>仅剩<text>{{ordersData[groupno].nums}}</text>个名额</view>
        <view class='c-info'>
          ￥<text>{{buytype==1?project.singleprice:project.groupprice}}</text>
          <!-- <text class='singleprice'>￥{{project.marketprice}}</text> -->
        </view>
        <view class='tobuy flex'>点击抢拼</view>
        <!-- <view class='c-date'>有效期：{{project.validate}}至{{project.validateend}}</view> -->
      </view>
    </view>
  </view>

  <view class='td-line'></view>

  <view class='avatar flex'>
    <view class='' wx:for="{{ordersData[groupno].pavatar}}">
      <image src='{{item}}'></image>
    </view>
    <view class='why flex' wx:for="{{lux}}">
      <image class='whypng' src="{{whypng}}"></image>
    </view>
  </view>

  <!-- 邀请好友 -->
  <block wx:if="{{ordersData[groupno].nums>0}}">
    <view class="share" hidden='{{!isshow}}' >
      <button hover-class="none" open-type="share">
        <text>邀请好友来拼团</text>
      </button>
    </view>
 </block>
 <block wx:else>
  <view class="nav">
      <view bindtap='toorder'>查看订单</view>
      <navigator url='../../../index/index' open-type="redirect" hover-class="none">
        <view>返回首页</view>
      </navigator>
  </view>
 </block>
  <!-- 商品信息 -->
  <view class="pinfo"  >
   <view class='pname flex'>
      <view class='left'>商品名称</view>
      <view class='right'>{{project.project}}</view>
   </view>
   <view class='pname flex'>
      <view class='left'>拼单时间</view>
      <view class='right'>{{ordersData[groupno].ptime}}</view>
   </view>
  
  </view>
 

   <view class='td-line'></view>
</block>
  
<!-- 分享按钮 
  <view class="share-box"  >
    <button hover-class="none" open-type="share">
      <text>分享</text>
    </button>
  </view>
-->

   <block wx:if="{{ad}}">
    <view class="ad-box">
      <image src="{{ad.adimg}}" lazy-load bindtap="jumpAd" data-adurl="{{ad.adurl}}" mode="aspectFill"></image>
    </view>
  </block>
</view>

