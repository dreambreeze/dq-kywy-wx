@import "../common/engin-nav.wxss";

.container {
  background-color: #eee;
  min-height: 1184rpx;
  padding-bottom: 100rpx;
}

.show-banner {
  height: 340rpx;
  position: relative;
}

.to-website {
  position: absolute;
  right: 0;
  top: 50%;
  width: 260rpx;
  height: 58rpx;
  line-height: 58rpx;
  margin-top: -29rpx;
  background: url(https://iservice.daqisoft.cn/Public/Home/images/to_website_bg_icon.png) no-repeat;
  font-size: 26rpx;
  color: #d1b58c;
  background-size: cover;
}

.to {
  padding-left: 100rpx;
}

.to-website-icon {
  display: inline-block;
  width: 15rpx;
  height: 18rpx;
  background: url(https://iservice.daqisoft.cn/Public/Home/images/to_website_icon.png) no-repeat;
  background-size: contain;
  vertical-align: middle;
  margin-left: 14rpx;
}

.banner swiper {
  height: 340rpx;
}

.item-image, .show-banner image {
  width: 100%;
  height: 100%;
}

.announcement-box {
  background: #fff;
}

.announcement {
  height: 56rpx;
  line-height: 56rpx;
  margin: 0 15rpx;
  overflow: hidden;
}

.ann-row {
  font-size: 26rpx;
  color: #e41515;
}

.top-bar-box {
  background: #eee;
}

.top-bar {
  width: 660rpx;
  padding: 28rpx 0;
  background-color: #333;
  box-sizing: border-box;
  margin: 28rpx auto;
  border-radius: 15rpx;
  box-shadow: 2rpx 10rpx 12rpx rgba(207, 178, 135, 0.3);
}

.top-bar-item {
  /* width: 50%; */
  flex: 1;
  text-align: center;
  font-size: 32rpx;
  color: #be9e70;
  height: 54rpx;
  line-height: 54rpx;
}

.top-bar-item image {
  vertical-align: middle;
  margin-right: 22rpx;
}

/* .top-bar-item:first-child {
  border-right: 1rpx solid #5c5c5c;
} */

.top-bar-item:first-child image {
  width: 42rpx;
  height: 44rpx;
}

/* .top-bar-item:last-child image {
  width: 44rpx;
  height: 42rpx;
} */

.grid-nav {
  background-color: #eee;
  box-sizing: border-box;
  padding: 28rpx 0 8rpx;
}

.grid-nav .flex-wrp {
  border-bottom: 1rpx solid #e8e8e8;
  background-color: #fff;
}

.grid-nav .flex-wrp:last-child {
  border: none;
}

.flex-wrp .nav-item:nth-child(3n) {
  border-right: none;
}

.nav-item {
  width: 250rpx;
  border-right: 1rpx solid #e8e8e8;
  height: 192rpx;
  text-align: center;
  font-size: 26rpx;
  color: #666;
  background-color: #fff;
}

.nav-item navigator {
  height: 100%;
}

.nav-item image {
  width: 54rpx;
  height: 52rpx;
  margin: 44rpx 0 16rpx;
}

.jishu-zhichi {
  /* margin-top: 35rpx; */
  position: fixed;
  bottom: 110rpx;
  left: 0;
  width: 100%;
}

/* 呼叫服务 */

.mask {
  background: rgba(0, 0, 0, 0.6);
}

.call-service {
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  height: 766rpx;
}

.donald-duck {
  position: absolute;
  width: 262rpx;
  height: 612rpx;
  left: 0;
  bottom: 230rpx;
  animation-fill-mode: forwards;
  -webkit-animation-fill-mode: forwards;
  animation-duration: 0.3s;
  -webkit-animation-duration: 0.3s;
  transform: translateX(-262rpx);
  -webkit-transform: translateX(-262rpx);
}

.donaldshowIn {
  animation-name: donaldshowIn;
  -webkit-animation-name: donaldshowIn;
}

@keyframes donaldshowIn {
  0% {
    transform: translateX(-262rpx);
    -webkit-transform: translateX(-262rpx);
  }

  100% {
    transform: translateX(0rpx);
    -webkit-transform: translateX(0rpx);
  }
}

@-webkit-keyframes donaldshowIn {
  0% {
    transform: translateX(-262rpx);
    -webkit-transform: translateX(-262rpx);
  }

  100% {
    transform: translateX(0rpx);
    -webkit-transform: translateX(0rpx);
  }
}

.donaldshowOut {
  animation-name: donaldshowOut;
  -webkit-animation-name: donaldshowOut;
}

@keyframes donaldshowOut {
  0% {
    transform: translateX(0rpx);
    -webkit-transform: translateX(0rpx);
  }

  100% {
    transform: translateX(-262rpx);
    -webkit-transform: translateX(-262rpx);
  }
}

@-webkit-keyframes donaldshowOut {
  0% {
    transform: translateX(0rpx);
    -webkit-transform: translateX(0rpx);
  }

  100% {
    transform: translateX(-262rpx);
    -webkit-transform: translateX(-262rpx);
  }
}

.donald-duck image {
  width: 100%;
  height: 100%;
}

.donald-duck-con {
  position: absolute;
  right: 0;
  bottom: 0;
  width: 488rpx;
  height: 765rpx;
  animation-fill-mode: forwards;
  -webkit-animation-fill-mode: forwards;
  animation-duration: 0.3s;
  -webkit-animation-duration: 0.3s;
  transform: translateX(488rpx);
  -webkit-transform: translateX(488rpx);
}


.donaldconshowIn {
  animation-name: donaldconshowIn;
  -webkit-animation-name: donaldconshowIn;
}

@keyframes donaldconshowIn {
  0% {
    transform: translateX(488rpx);
    -webkit-transform: translateX(488rpx);
  }

  100% {
    transform: translateX(0rpx);
    -webkit-transform: translateX(0rpx);
  }
}

@-webkit-keyframes donaldconshowIn {
  0% {
    transform: translateX(488rpx);
    -webkit-transform: translateX(488rpx);
  }

  100% {
    transform: translateX(0rpx);
    -webkit-transform: translateX(0rpx);
  }
}

.donaldconshowOut {
  animation-name: donaldconshowOut;
  -webkit-animation-name: donaldconshowOut;
}

@keyframes donaldconshowOut {
  0% {
    transform: translateX(0rpx);
    -webkit-transform: translateX(0rpx);
  }

  100% {
    transform: translateX(488rpx);
    -webkit-transform: translateX(488rpx);
  }
}

@-webkit-keyframes donaldconshowOut {
  0% {
    transform: translateX(0rpx);
    -webkit-transform: translateX(0rpx);
  }

  100% {
    transform: translateX(488rpx);
    -webkit-transform: translateX(488rpx);
  }
}

.donald_duck_title {
  width: 470rpx;
  height: 50rpx;
}

.service-box {
  margin-top: 60rpx;
  text-align: right;
}

.service-box scroll-view{
  height: 646rpx;
}

.service-list {
  display: inline-block;
  width: 340rpx;
  border-bottom: 1rpx solid #818181;
}

.service-list view {
  font-size: 30rpx;
  color: #ffd08d;
  height: 96rpx;
  line-height: 96rpx;
  border-top: 1rpx solid #818181;
  text-align: left;
  padding-left: 25rpx;
  /* border-image: url(https://iservice.daqisoft.cn/Public/Home/images/donald_duck.png) 0 0 rounded */
}
