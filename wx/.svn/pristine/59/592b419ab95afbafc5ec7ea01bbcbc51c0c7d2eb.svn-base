.h-search{
  height: 72rpx;
  width: 590rpx;
  margin: 20rpx 80rpx;
  background-color: #fff;
  justify-content: space-between;
  border: 1rpx solid rgba(0,0,0,0.1)
}
 .h-search .searchimg{
   width: 58rpx;
   justify-content: flex-end
 }
 .h-search image{
   width: 28rpx;
   height: 28rpx
 }
 .h-search input{
   width: 88%;
 }
 .h-search .searchsubmit{
  height:80rpx;
  width:128rpx;
  background-color:#eee;
  border-top-right-radius: 40rpx;
  border-bottom-right-radius: 40rpx;
  font-weight: bold;
  color: #d21717;
 }


.banner{
  height: 220rpx;
  margin: 10rpx 0 20rpx 0;
}
.swiper {
  height: 220rpx;
}
.swiper swiper-item .slide-image{
  width: 750rpx;
  height: 220rpx;
}



.bcline{
  height: 20rpx; 
  background-color: #eee;
}

.i-new{
  color: #666;
}
.i-new .n-head{
width:100%;
height:72rpx;
background-color: #eee;
margin-bottom: 10rpx;
justify-content:space-around;
}
.n-head .n-title{
  height: 100%;
  width:30%;
  text-align:center;
}
.n-head .n-title.typeactive{
  font-weight: bold
}
.n-head .n-title image{
  width: 12rpx;
  height: 22rpx;
  margin-left: 6rpx
}

.i-new .n-content{
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}
.i-new .n-item{
  padding: 10rpx 20rpx;
  position: relative;
}
.n-item image{
  width: 310rpx;
  height: 286rpx;
}
.n-item .t-iteminfo{
  position: absolute;
  width: 292rpx;
  height: 110rpx;
  background: rgba(255,255,255,0.7);
  bottom:34rpx;
  left:28rpx;
  text-align: center;
}
.t-iteminfo view:first-child{
  font-size:32rpx;
  font-weight: bold;
  color: #333;
  padding-top: 16rpx;
}

.h-content{
  padding: 0 10rpx; 
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.h-content .h-item{
  margin: 9rpx ; 
}
.h-item image{
  width: 345rpx;
  height: 334rpx;
}
.h-item .t-hinfo {
  margin-top: -14rpx;
  border: 1rpx solid rgba(0,0,0,0.1);
  padding-bottom: 10rpx;
}
.t-hinfo view:first-child{
 padding: 14rpx 10rpx 6rpx 10rpx;
}
.t-hinfo  view:last-child{
  padding: 0 10rpx;
  color: #db0000;
}


.nullgoods{
  height: 320rpx
}
.nomore{
  margin:50rpx 0 80rpx  0;
  color: #666;
  font-size: 24rpx;
}