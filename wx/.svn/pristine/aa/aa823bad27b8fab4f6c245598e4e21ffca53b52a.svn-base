@import "../../../common/store-address.wxss";
@import "../../../common/assess.wxss";

.container {
  background-color: #eee;
  padding-bottom: 100rpx;
}
.banner swiper{
  height: 424rpx;
}

.item-image {
  width: 100%;
  height: 100%;
}

.project-info {
  padding: 25rpx 20rpx;
  margin-bottom: 12rpx;
  background: #fff;
}

.project-name {
  font-size: 26rpx;
  color: #333;
  margin-bottom: 24rpx;
}

.project-name text {
  font-size: 32rpx;
  font-weight: bold
}
.project-name .peoples{
  padding:6rpx 10rpx;
  background-image:linear-gradient(to right bottom,#caa776 50%,#caa776 50%);
  border-radius:8rpx;
  font-size:22rpx;
  color:#fff;
  margin:0 12rpx 0 6rpx;
}

.project-price text:nth-of-type(1) {
  font-size: 38rpx;
  color: #e30101;
}

.project-price text:nth-of-type(1) text {
  font-size: 30rpx;
  margin-right: 8rpx;
}

.project-price text:nth-of-type(2) {
  font-size: 24rpx;
  color: #919090;
  margin-left: 22rpx;
  text-decoration: line-through;
}

.project-price text:nth-of-type(3) {
  float: right;
  font-size: 26rpx;
  color: #919090;
  margin-top: 10rpx;
}

.project-dil {
  background: #fff;
  padding: 0 20rpx;
  margin: 12rpx 0;
}

.dil-title {
  font-size: 28rpx;
  color: #666;
  height: 80rpx;
  display: flex;
  align-items: center;
}

.totips .dil-title{
  border-bottom: 1rpx solid rgba(0,0,0,0.1);

}
.techdetail{
  padding-bottom: 20rpx;
}
.td-intro{
  padding:10rpx 20rpx 2rpx 20rpx;
}
.td-head{
  height:50rpx;
  font-size: 26rpx;
  color: #f8ab11;
  display: flex;
  align-items: center;
  margin-left: -8rpx
}
.td-head view{
width:10rpx;
height:25rpx;
/* background-color:#caa876; */
background-image: linear-gradient(to right bottom,#f8ab11,#caa876);
margin-right: 8rpx;
border-radius: 8rpx;
}
.td-desc{
  margin-top:10rpx;
  line-height: 36rpx;
  font-size: 26rpx;
  /* text-indent: 8rpx; */
}
.totips .dil-content text{
  margin-top:10rpx;
  line-height: 36rpx;
  font-size: 26rpx;
  /* text-indent: 8rpx; */

}
.dil-content {
  font-size: 24rpx;
  color: #333;
  padding-bottom: 20rpx;
}

.look-all {
  width: 180rpx;
  height: 46rpx;
  line-height: 46rpx;
  text-align: center;
  font-size: 24rpx;
  color: #cdaa78;
  border: 1rpx solid #cdaa78;
  background: #fff;
  margin: 70rpx auto 180rpx;
  border-radius: 6rpx;
}

.buy-bar {
  width: 100%;
  height: 98rpx;
  background: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  box-sizing: border-box;
  text-align: center;
  font-size: 26rpx;
  z-index: 2;
}
.buy-bar .flex-wrp{
  justify-content: center;
  height: 98rpx;
}
.index{
  width: 28%;
  display:flex;
  justify-content:center;
  align-items:center;
  border-top: 1rpx solid rgba(0,0,0,0.1)
}
.onebuy,.buys{
  color: #fff;
  display:flex;
  justify-content:center;
  align-items:center;
  border-top: 1rpx solid rgba(0,0,0,0.1);
  width: 36%;

}
.buys{
  color: #caa876;
  background-color: #333;
}

.shidden{
  visibility: hidden;
}

.goindex image{
  width: 32rpx;
  height: 32rpx;
}