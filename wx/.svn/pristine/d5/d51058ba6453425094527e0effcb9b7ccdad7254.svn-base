/*选择支付方式 start */
  .container .swiper{
  height: 320rpx;
  background: #353535;
}
.swiper .sw-head{
  display:flex;
  align-items:center;
  height:60rpx;
  font-size:28rpx;
  color:#333;
  padding:10rpx 46rpx;
  border-bottom: 1rpx solid rgba(0,0,0,0.1);
}
.swiper swiper{
  height: 280rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid rgba(0,0,0,0.1);
}
.swiper .swiper-item{
  height: 160rpx;
  
  display: flex;
  justify-content: center;
  align-items: center;
  margin:  30rpx;
  border:  1rpx solid #eee;
  border-radius: 20rpx
}
.swiper .siactive{
   height: 180rpx;
   margin:  20rpx 65rpx;
   border:  1rpx solid #c59f68;
}
.swiper image{
  width: 110rpx;
  height: 80rpx;
}
.swiper .active{
  width: 132rpx;
  height: 92rpx;
}
.swiper .weactive{
  width: 80rpx;
  height: 80rpx;
}
.swiper-item .cardimg{
  margin: 0 30rpx 0 0
}
.swiper-item .cardinfo .cardname{
  font-size: 28rpx;
  color: #333;
}
.swiper-item .cardinfo .cardno{
  font-size: 22rpx;
  color: #dfceb5;
}

/*选择支付方式end  */

.content{
  height: auto;
  width: 100%;
  margin-top:20rpx;
}

.content .c-head{
  display: flex;
  align-items: center;
  height: 60rpx;
  font-size: 28rpx;
  color: #333;
}
.c-head .c-img{
  width: 27rpx;
  height:24rpx;
  padding: 0 10rpx 0 32rpx;
}
.c-head .c-img1{
  width: 18rpx;
  height:27rpx;
  padding: 6rpx 12rpx 0 10rpx;
}

.content .c-list{
  width: 100%;
  height: auto;
  border-bottom: 1rpx solid rgba(0,0,0,0.2);
  border-top:   1rpx solid rgba(0,0,0,0.2);
}
.c-list  .c-title{
  display: flex;
  align-items: center;
  font-size: 26rpx;
  color: #666;
  padding:10rpx 20rpx;
  border-bottom:1rpx solid rgba(0,0,0,0.1);
  height: 48rpx
}
.c-list  .t-info{
  flex: 3;
  text-align:center;
  height: 48rpx
}
.c-list .t-info image{
  width:35rpx;
height:35rpx;
position:absolute;
left:20rpx;
}
.c-list  .t-num{
  flex: 1;
  text-align:center;
   height: 48rpx
}
.c-list .c-item{
  height: 124rpx;
  padding: 20rpx 0 10rpx 0;
  margin: 0 20rpx;
  display: flex;
  border-bottom: 1rpx solid rgba(0,0,0,0.1);
  align-items: center;
}
.c-list .c-item:last-child{
  border: none;
}
.c-item .i-img{
  width: 88rpx;
  height: 88rpx;
  padding-left: 12rpx;
  margin-right: 24rpx;
}
.c-item .i-name{
  flex: 2;
  font-size: 28rpx;
  color: #333;
  max-width: 320rpx;
}
.c-item .i-num{
   flex: 1;
   font-size: 24rpx;
   color: #666;
   text-align:center;

}
.c-item .i-total{
   font-size: 24rpx;
   color: #333;
   flex: 1;
}
.c-item .selimg{
  height: 100%;
  width: 100rpx;
  display:flex;
  align-items:center;

}
.c-item .selimg image{
  width:35rpx;
  height:35rpx;
}

.content  .c-foot{
display:flex;
height:74rpx;
text-align:right;
align-items:center;
}
.c-foot .f-all {
flex: 1;
font-size:24rpx;
color:#666;
display:flex;
height:74rpx;
justify-content: flex-end;
align-items:center;
}
.c-foot .f-pay {
font-size:24rpx;
color:#666;
margin: 0 50rpx;
}
.c-foot .f-money{

font-size:28rpx;
color:#333;
font-weight:bold;
}
.line{
  height: 26rpx;
  background-color: #eee;
}
.lastline{
  height: 160rpx;
  background-color: #eee;
}

.note{
  display: flex;
  height: 88rpx;
  align-items: center;
}
.note .n-title{
  flex: 1;
  font-size:28rpx;
  color:#333;
  padding-left:32rpx;
  /* width: 88rpx; */
  width: 10%;
}
.note .n-desc{
  display:flex;
  align-items:center;
  font-size:26rpx;
  color:#666;
  /* width: 560rpx;
  justify-content:flex-end; */
  width: 82%;
  justify-content:flex-end;
}
.n-desc .n-note{
white-space:nowrap;
text-overflow:ellipsis;
overflow:hidden;


}
.note .n-lightimg{
  width: 12rpx;
  height: 22rpx;
  /* margin: 0 22rpx 0 32rpx; */
  padding: 0 22rpx 0 32rpx;
}

.topay{
position:fixed;
bottom:0; 
height:100rpx;
width: 100%;
display:flex;
align-items:center;
background-color: #fff;
}
.topay .t-title{
  flex: 1;
  margin-left: 32rpx;
  color: #666;
  font-size: 26rpx;
}
.t-title .t-total{
font-size:34rpx;
color:#db0000;
font-weight:bold;
}
.topay .t-sub{
  width: 240rpx;
   height: 100rpx;
  font-size: 34rpx;
}
.t-sub button{
  border: none;
  height: 100rpx;
  border-radius: 0;
}

/*选择支付方式  */
.payway, .payway .mask {
	position: fixed;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
}
.payway .mask {
	background: rgba(0,0,0,0.7);
}
.payway .list {
	position: fixed;
	left: 0;
	bottom: 0rpx;
	width: 100%;
	background: #f7f7f7;
	padding: 0rpx 0 50rpx 0;


}
.list .head{
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-bottom:1rpx solid rgba(0,0,0,0.1);
}
.head .ximg{
  position:absolute;
  left:0rpx;
  width:200rpx;
  height:80rpx;

}
.ximg image{
  padding: 30rpx;
  width: 22rpx;
  height: 22rpx;
}

.head .desc{
  font-size:32rpx;
  color: #333;
}

.list .price{
  height:210rpx;
display:flex;
align-items:center;
justify-content:center;
font-size:62rpx;
color:#333;
} 
.list .item{
display: flex;
display: -webkit-flex;
align-items: center;
height: 100rpx;
border-bottom: 1rpx solid rgba(0,0,0,0.1);
margin: 0 20rpx;
font-size: 30rpx;
color: #333;
}
.item .right{
  display: flex;
  align-items: center;
  justify-content: space-between
}
.item .pwl-name{
  flex: 2;
}
.item .pwl-img{
  width: 62rpx;
  height: 48rpx;
  padding: 0 28rpx 0 12rpx;
}
.item .pwl-lightimg{
  width: 12rpx;
  height: 18rpx;
  position: absolute;
  right: 38rpx;
}
.item .pwl-lightimg.seleted-img{
  width: 26rpx;
  height: 26rpx;
}
.list .forcard{
  height: 128rpx
}
.list .cardinfo{
  display:flex;
  display:-webkit-flex;
  align-items:center;
  margin:-30rpx 20rpx 0 20rpx;
  font-size:22rpx;
  color:#dfceb5;
}
.list .cardinfo .cardno{
  margin:0 10rpx 0 100rpx
}

.payway .pw-sub{
  height: 96rpx;
  width: 100%;
  margin-top: 200rpx;
  display: flex;
  justify-content: center;
}
.pw-sub .blue{
  width: 660rpx;
  color: #fff;
  font-size: 34rpx;
  font-weight: bold;
background-image:-webkit-linear-gradient(to bottom right, #dfceb5 , #c59f68); 
background-image:linear-gradient(to bottom  right,#dfceb5 , #c59f68);
}

.wxpay{
font-size:20rpx;
color:#caa876;
border:1rpx solid rgba(222, 204, 178, 0.8);
padding:4rpx 12rpx;
border-radius:10rpx;
width:82rpx;
margin: 4rpx 0;
}
