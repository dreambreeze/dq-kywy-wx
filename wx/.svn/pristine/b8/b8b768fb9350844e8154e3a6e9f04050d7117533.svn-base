<view class='container'>
  <view class='head'>
    <view class='shopimg'> <image src="https://iservice.daqisoft.cn/Public/Home/images/techimgs/shop.png"></image></view>
    <view class='shopname'>
      <view >Healthy体育新城店</view>
      <view>您有1张优惠券待领取</view>
    </view>
    <view class='changeshop'>
      切换门店 <image src ="https://iservice.daqisoft.cn/Public/Home/images/techimgs/arrow.png"></image>
    </view>
  </view>
  
  <view class='shopinfo'>
    <view class='s-addr'>
        <image src ="https://iservice.daqisoft.cn/Public/Home/images/techimgs/addr.png"></image>
        长沙市芙蓉区万家丽中路1369号
    </view>
    <view class='s-distance'>
    1369km
    <image src = "https://iservice.daqisoft.cn/Public/Home/images/techimgs/direc.png"></image>
    </view>
    <view class='s-phone'>
      <image src = "https://iservice.daqisoft.cn/Public/Home/images/techimgs/phone.png"></image>
    </view>
  </view>

  <view class='t-line'></view>

  <view class='content'>
      <view class='c-search'>
        <input type='text' confirm-type='search' placeholder='请输入技师号' placeholder-class='plhc' cursor='50' bindfocus='bindfocus' value='{{techNODefault}}'></input>
      </view>
      <view class='c-class'>
      <view class='c-item {{typeDefault==0?"active":""}}' data-id="0" bindtap='typeSelect'>全部</view>
      <block wx:for="{{typeLists}}" wx:for-index="tl" wx:key="tlid">
       <view class='c-item {{typeDefault==item.id?"active":""}}' data-id="{{item.id}}" bindtap='typeSelect'>{{item.name}}</view>
      </block>
     
        <!-- <view class='c-item {{typeDefault==0?"active":""}}' data-id="0" bindtap='typeSelect'>全部</view>
        <view class='c-item {{typeDefault==1?"active":""}}' data-id="1" bindtap='typeSelect'>SPA</view>
        <view class='c-item {{typeDefault==2?"active":""}}' data-id="2" bindtap='typeSelect'>中医</view>
        <view class='c-item {{typeDefault==3?"active":""}}' data-id="3" bindtap='typeSelect'>泰式</view>
        <view class='c-item {{typeDefault==4?"active":""}}' data-id="4" bindtap='typeSelect'>普足</view>
        <view class='c-item {{typeDefault==5?"active":""}}' data-id="5" bindtap='typeSelect'>按摩理疗</view> -->
      </view>

      <view class='t-info'>
        <view class='t-item' bindtap='toproject' data-id='102'>
