<view class="container">
  <view class="banner">
    <swiper indicator-dots="true" autoplay="true" interval="3000" duration="400" indicator-color="rgba(255, 255, 255, .5)" indicator-active-color="#caa876">
      <block wx:for="{{!reserveProject[0].picurlarr ? banner : reserveProject[0].picurlarr}}">
        <swiper-item>
          <image src="{{!reserveProject[0].picurlarr ? item : showImgUrl + item}}" class="item-image" mode="aspectFill"></image>
        </swiper-item>
      </block>
    </swiper>
  </view>

  <view class="project-info">
    <view class="project-name">
      <text>{{reserveProject[0].serviceitemname}}</text>（{{reserveProject[0].servicetime}}分钟）</view>
    <view class="project-price">
      <text><text>¥</text>{{reserveProject[0].pturn}}</text>
      <!-- <text>门市价 ¥ 399.00</text>  -->
      <!-- <text>已售170笔</text> -->
    </view>
  </view>

  <!-- 门店地址模版 -->
  <!-- <template is="store-address" data="{{storename: '和毅人民路店',storeaddress: '岳麓区当代滨江MOMA四楼当代滨江MOMA四楼'}}" /> -->

  <view class="project-dil">
    <view class="dil-title">项目介绍</view>
    <view class="dil-content">
      <text>{{!reserveProject[0].mminfo ? '无项目介绍' : reserveProject[0].mminfo}}</text>
    </view>
  </view>

  <view class="buy-bar" bindtap="reserveBtn">返回</view>
</view>

<!-- 返回首页按钮 -->
<view class="backhome-btn">
  <view>
    <navigator url="/pages/index/index" hover-class="none" open-type="reLaunch">
      <text>返回首页</text>
    </navigator>
  </view>
</view>