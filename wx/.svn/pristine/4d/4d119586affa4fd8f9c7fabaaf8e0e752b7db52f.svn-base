<import src="../../../common/coupons.wxml" />

<view class="container">
  <view class="coupon-box">
    <!-- 卡券内容模版 -->
    <template is="coupons" data="{{coupons:[0,1,2],display:display,ishide:ishide,btnName: '立即使用',arrow: arrow,cAction: 'active'}}" />

    <view class="empty-coupon" style="display:none">
