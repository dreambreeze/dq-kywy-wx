<view class="container">
  <view class="banner">
    <swiper indicator-dots="true" autoplay="true" interval="3000" duration="400" indicator-color="rgba(255, 255, 255, .5)" indicator-active-color="#caa876">
      <block wx:for="{{banner}}">
        <swiper-item>
          <image src="{{item}}" class="item-image" mode="aspectFill"></image>
        </swiper-item>
      </block>
    </swiper>
  </view>

  <view class="project-info">
    <view class="project-name">
      <text>美丽佳人</text>（90分钟）</view>
    <view class="project-price">
      <text><text>¥</text>339.00</text>
      <text>门市价 ¥ 399.00</text>
      <!-- <text>已售170笔</text> -->
    </view>
  </view>

  <!-- 门店地址模版 -->
  <!-- <template is="store-address" data="{{storename: '和毅人民路店',storeaddress: '岳麓区当代滨江MOMA四楼当代滨江MOMA四楼'}}" /> -->

  <view class="project-dil">
    <view class="dil-title">项目介绍</view>
    <view class="dil-content">
      <text>时间：90分钟\n组成：开背放松+肩颈腰背臀部+要不艾灸+腿部+面部清洁+头部放松\n功效：预防及改善因肾气下降引起的肾虚、体力衰弱、健忘、记忆力减退预防及改善因肾气下降引起的肾虚、体力衰弱、健忘、记忆力减退</text>
    </view>
  </view>

  <view class="buy-bar" bindtap="reserveBtn">预约</view>
</view>

<!-- 分享按钮 -->
<view class="share-box">
  <button hover-class="none" open-type="share">
    <text>分享</text>
  </button>
</view>