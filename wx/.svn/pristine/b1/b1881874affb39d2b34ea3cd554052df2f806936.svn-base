page {
	background-color: #fff;
  padding-bottom: 120rpx
}
.head{
  height: 200rpx;

}
.head .black{
  height: 100rpx;
  background-color: #353535;
}
.head .white{
  height: 100rpx;

}
.head .h-content{
  position: absolute;
  width: 180rpx;
  height: 180rpx;
  background-color: #fff;
 
  border-radius: 50%;
  left:38%;
  top:10rpx;
}
.h-content .h-in{
  width: 140rpx;
  height: 140rpx;
  margin: 10rpx;
  border-radius: 50%;
  border: 10rpx solid #c9aa77;
}
.h-content .h-time{
  margin-top: 20rpx;
  font-size: 42rpx;
  color: #333;
  font-weight: bold;
  display: flex;
  justify-content: center;
}
.h-content .h-desc{
  margin-top: 0rpx;
  font-size: 22rpx;
  color: #999;
  display: flex;
  justify-content: center;
}

.orderstatus{
  display:flex;
justify-content:center;
margin-top:36rpx;
font-size:34rpx;
font-weight:bold;

}
.orderfast{
display:flex;
justify-content:center;
margin-top:46rpx;
font-size:26rpx;
color: #666;
}
.orderfast text{
  height: 60rpx;
  width:140rpx;
  border: 1rpx solid rgba(0,0,0,0.3);
  border-radius: 4rpx;
  text-align: center;
  line-height: 60rpx;
  margin-bottom: 20rpx;
}
.nav {
  display: flex;
  justify-content: center;
  font-size: 26rpx;
  color: #525252;
}
.nav view{
  height: 52rpx;
  width: 146rpx;
  border: 1rpx solid rgba(0, 0, 0, 0.3);
  border-radius: 8rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #666;
  margin-bottom: 40rpx ;
  
}
.nav view:last-child{
   border: 1rpx solid #caa876;
   background-color: #f4f4e4;
   margin-left: 40rpx;
}

.line{
  width: 100%;
  height: 22rpx;
  background-color: #eee;
  
}
.content{
  height: auto;
  width: 100%;
}

.content .c-head{
  display: flex;
  align-items: center;
  height: 60rpx;
  font-size: 28rpx;
  color: #333;
}
.c-head .c-img{
  width: 27rpx;
  height:24rpx;
  padding: 0 10rpx 0 32rpx;
}
.c-head .c-img1{
  width: 18rpx;
  height:27rpx;
  padding: 6rpx 12rpx 0 10rpx;
}

.content .c-list{
  width: 100%;
  height: auto;
  border-bottom: 1rpx solid rgba(0,0,0,0.2);
  border-top:   1rpx solid rgba(0,0,0,0.2);
}
.c-list .c-item{
  height: 124rpx;
  padding: 20rpx 0 10rpx 0;
  margin: 0 50rpx;
  display: flex;
  border-bottom: 1rpx solid rgba(0,0,0,0.1);
  align-items: center;
}
.c-item .i-img{
  width: 88rpx;
  height: 88rpx;
  padding-left: 12rpx;
  margin-right: 24rpx;
}
.c-item .i-name{
  flex: 2;
  font-size: 30rpx;
  color: #333;
}
.c-item .i-num{
    flex:3; 
   font-size: 24rpx;
   color: #666;
}
.c-item .i-total{
   font-size: 26rpx;
   color: #333;
}
.content  .c-foot{
display:flex;
height:74rpx;
text-align:right;
align-items:center;
}
.c-foot .f-all {
flex: 1;
font-size:24rpx;
color:#666;
}
.c-foot .f-pay {
font-size:24rpx;
color:#666;
margin: 0 50rpx;
}
.c-foot .f-money{
font-size:28rpx;
color:#333;
font-weight:bold;
}
.line{
  height: 26rpx;
  background-color: #efefef;
}

.order-info {
height: auto;
font-size: 28rpx;
color: #333;
}
.order-info .o-head{
  height: 80rpx;
  display: flex;
  align-items: center;
  padding-left: 32rpx;
  border-bottom: 1rpx solid rgba(0,0,0,0.2);
}
.order-info .o-content{
  padding:20rpx 42rpx;
  display: flex;
  padding-bottom: 100rpx;
}
.o-content .o-desc{
  height: 48rpx;
}
.o-content .o-left{
  width: 152rpx;
  text-align: right;
  color: #666;
}
.o-content .o-left view{
  width: 152rpx;
  height: 48rpx;
}
.o-content .o-right{
  width: 360rpx;
  text-align: left;
}
.o-content .o-right view{
  width: 500rpx;
  height: 48rpx;
  word-break:break-all;
}
.o-content .o-right view:first-child{
  color: #fe7023;
}