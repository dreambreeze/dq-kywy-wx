@import "../../../common/project.wxss";

.container {
  background-color: #eee;
  padding-bottom: 98rpx;
  min-height: 1086rpx;
}

.top-search {
  height: 88rpx;
  background: #fff;
  position: relative;
  overflow: hidden;
  margin-bottom: 12rpx;
}

.searchs {
  width: 480rpx;
  height: 62rpx;
  line-height: 62rpx;
  border-radius: 40rpx;
  margin: 13rpx auto 0;
  background: #e6e4e5;
  font-size: 26rpx;
  text-align: center;
  color: #9b9b9b;
}

.searchs view {
  display: inline-block;
  width: 28rpx;
  height: 28rpx;
  background: url(https://iservice.daqisoft.cn/Public/Home/images/search.png) no-repeat;
  background-size: contain;
  vertical-align: middle;
  margin-top: -5rpx;
  margin-right: 6rpx;
}

.region {
  height: 62rpx;
  line-height: 62rpx;
  position: absolute;
  top: 13rpx;
  left: 26rpx;
  color: #c7ab84;
  font-size: 30rpx;
}

.region view {
  width: 0;
  height: 0;
  display: inline-block;
  border-width: 4px 4px 0 4px;
  border-style: solid;
  border-color: #c7ab84 transparent transparent transparent;
  vertical-align: middle;
  margin-left: 4px;
}

.store-item {
  margin-bottom: 14rpx;
  background: #fff;
  overflow: hidden;
}

.store-title {
  height: 70rpx;
  line-height: 70rpx;
  border-bottom: 1rpx solid #cdcccc;
  padding: 0 28rpx;
}

.store-ico {
  display: inline-block;
  width: 26rpx;
  height: 24rpx;
  background: url(https://iservice.daqisoft.cn/Public/Home/images/store_ico1.png) no-repeat;
  background-size: contain;
  vertical-align: middle;
  margin-right: 16rpx;
}

.store-name {
  font-size: 28rpx;
  color: #333;
}

.jump-ico {
  display: inline-block;
  width: 11rpx;
  height: 17rpx;
  background: url(https://iservice.daqisoft.cn/Public/Home/images/arrowright.png) no-repeat;
  background-size: contain;
  margin-left: 38rpx;
}

.distance {
  float: right;
  color: #999;
  font-size: 24rpx;
}


.look-more {
  height: 78rpx;
  line-height: 78rpx;
  font-size: 26rpx;
  color: #c7ab84;
  text-align: center;
  border-top: 1rpx solid #e6e5e5;
}

.arrowdown, .arrowup {
  width: 15rpx;
  height: 7rpx;
  margin-left: 14rpx;
  display: inline-block;
  vertical-align: middle;
  margin-top: -4rpx;
}

.arrowdown {
  background: url(https://iservice.daqisoft.cn/Public/Home/images/arrowdown1.png) no-repeat;
  background-size: contain;
}

.arrowup {
  background: url(https://iservice.daqisoft.cn/Public/Home/images/arrowup1.png) no-repeat;
  background-size: contain;
}

.item{
  margin:30rpx;
}
.item .i-img image{
  height: 390rpx;
  width: 690rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}
.item .i-bottom{
  border:1rpx solid rgba(0,0,0,0.1);
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}