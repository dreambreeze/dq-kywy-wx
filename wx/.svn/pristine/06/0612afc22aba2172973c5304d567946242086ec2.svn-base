<view class='amcontainer'>
  <view class='head'>
      <view class='h-img'>
        <image  src='{{compeleteimg}}'></image>
      </view>
      <view class='h-desc'>支付成功!</view>
       <view class='h-time'>预计<text >{{arrivetime}}分钟</text>送达</view>
  </view>
  
  <view class="nav">
    
      <view bindtap='toorder'>查看订单</view>
      <navigator url='../detail/detail' open-type="redirect" hover-class="none">
        <view>继续逛逛</view>
      </navigator>
  </view>

  <view class="ad">
     <image src='{{adimg}}'></image>
  </view>
</view>

