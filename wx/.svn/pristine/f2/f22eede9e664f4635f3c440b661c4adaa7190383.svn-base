page {
  height: 100%;
  overflow: hidden;
} 
.container{
  height: 100%;
}
.yscroll{
  height: 100%;
  width: 100%;
}

.container {
	color: #333;
	font-size: 28rpx
}

.container .sc-head {
	position: relative;
	width: 750rpx;
	height: 362rpx
}

.sc-head .h-red {
	width: 100%;
	height: 198rpx;
	background-color: #cf0100
}

.sc-head .h-white {
	width: 100%;
	height: 164rpx
}

.sc-head .h-content {
	position: absolute;
	bottom: 20rpx;
	left: 58rpx;
	width: 634rpx;
	height: 278rpx;
	border-radius: 6rpx;
	background-color: #fff;
	box-shadow: 0 12rpx 40rpx rgba(0,0,0,.1)
}

.h-content .c-head {
	display: flex;
  justify-content: space-between;
	height: 64rpx;
	border: 0;
	background-color: #cf0100
}

.c-head .c-express,.c-head .c-take {
	width: 282rpx;
	border: none;
  border-top-right-radius: 6rpx;
	border-top-left-radius: 6rpx;
	background-color: hsla(0,0%,100%,.5);
	text-align: center;
	font-size: 30rpx;
	line-height: 64rpx
}

 .c-head .c-express.active view,.c-head .c-take.active view {
	margin-top: -94rpx;
	color: #d21717;
	font-weight: 700;
	font-size: 32rpx
} 
 .c-head .c-take.active image{margin-left: -24rpx}
 .c-head image{
   width: 306rpx;
   height: 72rpx
 }


.h-content .c-info {
  height: 142rpx;
	padding: 26rpx 20rpx
}

.c-info .i-title {
	color: #666;
	text-align: center;
	font-size: 26rpx
}
.c-info .choosestore{
  height: 172rpx;
  font-size: 30rpx;
  justify-content: center
}
.c-info .choosestore image{
  width: 12rpx;
  height: 19rpx;
  margin-left: 10rpx
}
.c-info .choosed{
  height: 100%;
  width: 100%
}
.choosed .a-pak.cd-pak{
  text-align: left
}
.choosed .i-addr .a-addr.a-storename{padding-bottom: 10rpx;font-size: 30rpx}
.choosed .ad-fulladdr{font-size: 26rpx;justify-content: flex-start}
.choosed .ad-fulladdr .addrimg{width: 46rpx;}
.choosed .ad-fulladdr image{width: 29rpx;height: 35rpx;margin:0 8rpx 0 0}
.c-info .i-addr {
	display: flex;
	padding-top: 24rpx;
	justify-content: center;
	align-items: center;
  min-height: 120rpx
}

.i-addr .a-pak {
	width: 534rpx;
	text-align: center;
	flex: 1
}

.i-addr .a-pak .a-addr {
	overflow-x: hidden;
	padding-bottom: 20rpx;
	width: 534rpx;
	text-overflow: ellipsis;
	white-space: nowrap
}

.i-addr .a-pak .a-phone {
	width: 534rpx
}

.i-addr .a-right image {
	width: 12rpx;
	height: 19rpx
}


.content{
  height: auto;
  width: 100%;
  margin-top:20rpx;
}

.content .c-head{
  display: flex;
  align-items: center;
  height: 60rpx;
  font-weight: bold;
}
.c-head .c-img{
  width: 27rpx;
  height:24rpx;
  padding: 0 16rpx 0 26rpx;
}
.c-head .c-img1{
  width: 12rpx;
  height:19rpx;
  padding: 6rpx 0 0 10rpx;
}

.content .c-list{
  width: 100%;
  height: auto;
  border-top:   1rpx solid rgba(0,0,0,0.2)
}
.c-list .c-item{
  height: 104rpx;
  padding: 20rpx 0 20rpx 0;
  margin: 0 26rpx;
  display: flex;
  border-bottom: 1rpx solid rgba(0,0,0,0.1);
  align-items: center;
}
.c-list view:last-child{
  border: none;
}
.c-item .i-box{width: 100%}
.c-item .i-box .flex{justify-content: space-between}
.c-item .i-info{
  display: flex;
  align-items: center;
  justify-content: space-between
}
.i-info .i-desc{
  font-size: 22rpx;
  color: #999;
}
.c-item .i-img{
  width: 88rpx;
  height: 88rpx;
  padding-left: 12rpx;
  margin-right: 24rpx;
}
.c-item .i-name{
  font-size: 30rpx;
  color: #333;
}
.c-item .i-num{
   font-size: 24rpx;
   color: #666;
}
.c-item .i-total{
   font-size: 26rpx;
   color: #333;
}

.config{
  height: 172rpx;
  padding: 0 32rpx;
}
.config .c-coupon{
  height: 86rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid rgba(0,0,0,.1); 
}
.config .sc-title{
  flex:1;
}
.config .sc-num{
  width: 100rpx;
  height: 32rpx;
  background: #ee7201;
  color: #fff;
  line-height: 30rpx;
  text-align: center;
  border-radius: 4rpx;
  font-size: 22rpx
}
.c-coupon .sc-right{
  display: flex;
  align-items: center;
}
.c-coupon .sc-right image{
  width: 14rpx;
  height: 26rpx;
  margin-left: 14rpx
}
.note {
	display: flex;
	height: 88rpx;
  border-bottom:1rpx solid rgba(0,0,0,0.1);
  margin-bottom: 270rpx;
	align-items: center
}

.note .n-title {
	flex: 1;
	font-size: 28rpx;
	color: #333;
	padding-left: 32rpx;
	width: 10%
}

.note .n-desc {
	display: flex;
	align-items: center;
	font-size: 26rpx;
	color: #666;
	width: 82%;
	justify-content: flex-end
}

.n-desc input {width: 100%}

.topay {
  z-index: 1;
	position: fixed;
	bottom: 0;
	height: 98rpx;
	width: 100%;
	display: flex;
	align-items: center;
  border-top: 1rpx solid rgba(0,0,0,0.1);
	background-color: #fff
}

.topay .t-coupon{
  margin: 0 30rpx;
  color: #666;
  font-size: 24rpx;
}
.topay .t-title {
  flex: 1;
	text-align: right;
	margin: 0 24rpx;
	font-size: 30rpx
}
.t-title .t-total {
	color: #d21717;
	font-weight: 700
}

.topay .t-sub {
	width: 264rpx;
	height: 98rpx;
  font-weight: bold;
	font-size: 34rpx
}

.t-sub button {
  background-color: #d21717;
	border: none;
	color: #fff;
  height: 98rpx;
	border-radius: 0
}

/*选择门店所有门店  */
.allstore, .allstore .mask {
  z-index: 2;
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
}
.allstore .mask {
	background: rgba(0,0,0,0.7);
}
.allstore .close{
  width: 72rpx;
  height: 100rpx;
  position:absolute;
  right:110rpx;
  top:164rpx;
  z-index: 4;
  flex-direction:column;
justify-content:flex-end;
}
.close image{
  width: 58rpx;
  height: 58rpx
}
.close .line{
  width: 4rpx;
  height: 16rpx;
  background-color: #fff
}
.allstore .storelist{
position:absolute;
background-color:#fff;
width:530rpx;
max-height:680rpx;
z-index:3;
border-radius:4rpx;
left:110rpx;
top:260rpx;
}
.storelist .storeitem{
padding:20rpx 20rpx;
height:78rpx;
border-bottom:1rpx solid rgba(0,0,0,0.1)
}
.storeitem .storeaddr{
  font-size: 24rpx;
  color: #666;
  margin-top: 10rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden
}

/*选择支付方式  */
.payway, .payway .mask {
	position: fixed;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
  
}
.payway{
  z-index: 3
}
.payway .mask {
	background: rgba(0,0,0,0.7);
}
.payway .list {
	position: fixed;
	left: 0;
	bottom: 0rpx;
	width: 100%;
	background: #f7f7f7;
	padding: 0rpx 0 50rpx 0;


}
.list .head{
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-bottom:1rpx solid rgba(0,0,0,0.1);
}
.head .ximg{
  position:absolute;
  left:34rpx;
  line-height:80rpx;
}
.ximg image{
  width: 22rpx;
  height: 22rpx;
}
.head .desc{
  font-size:32rpx;
  color: #333;
}

.list .price{
  height:210rpx;
display:flex;
align-items:center;
justify-content:center;
font-size:62rpx;
color:#333;
} 
.list .item{
display: flex;
display: -webkit-flex;
align-items: center;
height: 100rpx;
border-bottom: 1rpx solid rgba(0,0,0,0.1);
margin: 0 20rpx;
font-size: 30rpx;
color: #333;
}
.item .pwl-name{
  flex: 2;
}
.item .pwl-img{
  width: 62rpx;
  height: 48rpx;
  padding: 0 28rpx 0 12rpx;
}
.item .pwl-lightimg{
  width: 12rpx;
  height: 18rpx;
  position: absolute;
  right: 38rpx;
}
.item .pwl-lightimg.seleted-img{
  width: 26rpx;
  height: 26rpx;
}
.payway .pw-sub{
  height: 96rpx;
  width: 100%;
  margin-top: 200rpx;
  display: flex;
  justify-content: center;
}
.pw-sub .blue{
  width: 660rpx;
  color: #fff;
  font-size: 34rpx;
  font-weight: bold;
background-image:-webkit-linear-gradient(to bottom right, #e23f19 , #d52f2e); 
background-image:linear-gradient(to bottom  right, #e23f19 , #d52f2e);
}
