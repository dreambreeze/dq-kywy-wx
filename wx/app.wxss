/**app.wxss**/
@import "commons/wxss/icon.wxss";
@import "commons/wxss/icon_size.wxss";
page {
    background: #f5f5f5;
    line-height: 1.5;
    color:#333;
}

view, scroll-view {
    box-sizing: border-box;
}

button::after {
    border: none;
}

/* 文字禁止换行公用类 */

.text-ellipsis, .fun-title, .s-info-b view {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.nowrap{
  text-overflow: ellipsis;
  white-space: nowrap;
   overflow: hidden;
}
/* 横向布局 */

.flex-wrp {
    display: flex;
    flex-direction: row;
}

image {
    vertical-align: top;
}

/*设置*/

.go-settimg {
    display: inline-block;
    margin-left: 4rpx;
    color: #006cbd;
}

/* zhz */

.mask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.3);
}

.btn-gradient {
    background-image: -webkit-linear-gradient(300deg, #77bdff 20%, #6fb9fd 40%, #48a1f4 60%, #3093ef 80%, #238bec 100%) !important;
    background-image: -o-linear-gradient(300deg, #77bdff 20%, #6fb9fd 40%, #48a1f4 60%, #3093ef 80%, #238bec 100%) !important;
    background-image: -moz-linear-gradient(300deg, #77bdff 20%, #6fb9fd 40%, #48a1f4 60%, #3093ef 80%, #238bec 100%) !important;
    background-image: -ms-linear-gradient(300deg, #77bdff 20%, #6fb9fd 40%, #48a1f4 60%, #3093ef 80%, #238bec 100%) !important;
}

/* 清除浮动 */

.clear:after, .clear:before {
    content: "";
    display: table;
}

.clear:after {
    clear: both;
}

/* 分享按钮公用 */

.share-box {
    position: fixed;
    right: 0;
    bottom: 126rpx;
    text-align: center;
}

.share-box text {
    display: inline-block;
    line-height: 1;
    margin-top: 66rpx;
}

.share-box button {
    display: inline-block;
    width: 116rpx;
    height: 116rpx;
    text-align: center;
    font-size: 26rpx;
    color: #c7b498;
    border-radius: 50%;
    font-weight: bold;
    border: none;
    padding: 0;
    box-sizing: initial;
    box-shadow: 0 2px 6px rgba(88, 88, 88, 0.3);
    background: #585858 url(https://iservice.daqisoft.cn/Public/Home/images/share_icon.png) no-repeat center 20rpx;
    background-size: 34%;
}

.share-box button::after, .getPhoneNumber button::after, .btn-gradient::after,
.again-reserve button::after {
    content: normal;
}

.back-box {
    position: fixed;
    right: 0;
    bottom: 126rpx;
    width: 116rpx;
    height: 116rpx;
    font-size: 26rpx;
    color: #c7b498;
    border-radius: 50%;
    font-weight: bold;
    border: none;
    padding: 0;
    box-sizing: initial;
    box-shadow: 0 2px 6px rgba(88, 88, 88, 0.3);
    background: #585858;
    background-size: 34%;
}

.back-box .back {
    display: flex;
    justify-content: center;
    align-items: center;
    width: 116rpx;
    /* width: 56rpx;  */
    height: 116rpx;
    border-radius: 50%;
}

/* 技术支持大旗软件 */

.jishu-zhichi {
    text-align: center;
    font-size: 24rpx;
    color: #9e9e9e;
    padding: 10rpx 0;
}

/*自助点单 在线商城  */

.amcontainer {
    height: 100%;
    overflow: hidden;
    background-color: #fff;
    font-family: '华文细黑';
}

/*特殊按钮 #1E90FF*/

button.blue {
    color: #caa876;
    background: #353535;
}

button.blue:after {
    border-color: rgba(255, 133, 111, 0.3);
}

button.blue.hover {
    background: #353535;
}

button.blue.disabled {
    color: #fff;
    font-size: 34rpx;
    background: #b4b4b4;
}

/*约技师  */

.techsubmit {
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 90rpx;
    font-size: 30rpx;
    color: #fff;
    font-weight: bold;
    text-align: center;
    line-height: 90rpx;
    background: #3b81d6;
}

.td-line {
    height: 20rpx;
    background: #eee;
}

.empty-assess, .notmodule {
    text-align: center;
    padding: 20rpx 0;
    font-size: 30rpx;
    color: #999;
}

.notmodule-box {
    text-align: center;
    padding-top: 26rpx;
}

/* 在线商城  */

.shopcontainer {
    height: 100%;
    padding: 0;
    margin: 0;
    font-family: "华文细黑";
    color: #333;
    font-size: 28rpx;
}

.bcline {
    height: 20rpx;
    background-color: #eee;
}

.flex {
    display: flex;
    align-items: center;
    justify-content: center;
}

.ellipsis {
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}

/* 返回首页按钮 */

.back-home{
    position: fixed;
    right: 10rpx;
    bottom: 120rpx;
    text-align: center;
    background: url('https://note.youdao.com/yws/api/personal/file/WEBffc1d0c0819b704cf4e4ecf5b705b798?method=download&shareKey=f6e1abfa453a1bc5901439278728d52b') no-repeat center;
    background-size:90rpx;
    width:90rpx;
    height: 90rpx;
}

/*广告  */

.ad-box image {
    width: 100%;
    height: 230rpx;
}

/* 已展示全部内容 */
.no-more{
    line-height:100rpx;
    font-size:24rpx;
    color:#aaaaaa;
    text-align: center;
    position: relative;
}

.no-more::before{
    content: '';
    width:150rpx;
    height:1rpx;
    background: #aaaaaa;
    position: absolute;
    top:50%;
    left: 50rpx;
}

.no-more::after{
    content: '';
    width:150rpx;
    height:1rpx;
    background: #aaaaaa;
    position: absolute;
    top:50%;
    right: 50rpx;
}

.gray-font{
    color:#999;
}
.tips-text{
  color: #999;
  font-size: 28rpx;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx
}