@import "../common/engin-nav.wxss";

/*html内容转义插件样式*/
@import "../../wxParse/wxParse.wxss";

.container {
    background-color: #eee;
    min-height: 1184rpx;
    padding-bottom: 100rpx;
}

/* banner轮播图 start */

.swiper {
    height: 340rpx;
    background: url('https://iservice.daqisoft.cn/Public/Home/images/newimages/index_banner_back.png') repeat-x;
    background-size: auto 380rpx;
    padding: 0 0 40rpx;
    position: relative;
    overflow: hidden;
}

.contact-tools {
    position: absolute;
    left: 0;
    right: 0;
    font-size: 20rpx;
    margin: -40rpx 30rpx 0;
    color: #656565;
    display: flex;
    line-height: 40rpx;
    border-bottom: 1rpx #ddd solid;
}

.contact-tools text:nth-of-type(1) {
    flex: 1;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.tel-icon {
    display: inline-block;
    width: 40rpx;
    height: 40rpx;
    background: url('https://iservice.daqisoft.cn/Public/Home/images/newimages/index_tel@2x.png') no-repeat center;
    background-size: 22rpx;
}

.swiper swiper {
    height: 340rpx;
}

.swiper swiper-item {
    display: flex;
    justify-content: center;
    align-items: center;
}

.swiper image {
    width: 450rpx !important;
    height: 161rpx;
    opacity: 0.5;
    z-index: 2;
    border-radius: 10rpx;
}

.swiper .active {
    width: 500rpx !important;
    height: 245rpx;
    transition: all 0.2s ease-in-out 0.3s;
    opacity: 1;
    z-index: 3;
}

.function-list {
    display: flex;
    background: #fff;
    padding: 30rpx;
    border-top: 1rpx #f5f1f1 solid;
    font-size: 24rpx;
    justify-content: space-around;
}

.function-list .item {
    flex: 1;
    text-align: center;
}

.function-list .item image {
    width: 80rpx;
    height: 80rpx;
    margin: 0 0 15rpx 0;
    border-radius: 50%;
}

.message-box {
    margin: 14rpx 0;
    padding: 10rpx 30rpx 10rpx 106rpx;
    overflow: hidden;
    background: #fff;
}

.message-box image {
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    margin-left: -76rpx;
}

.message-list {
    font-size: 18rpx;
    height: 60rpx;
}

.message-list .item {
    height: 25rpx;
    line-height: 25rpx;
    margin-bottom: 10rpx;
    display: flex;
    align-items: center;
}

.message-list .red-tag {
    color: #e54837;
    border: #e54837 1rpx solid;
}

.message-list .green-tag {
    color: #53a64e;
    border: #53a64e 1rpx solid;
}

.message-list .tag {
    display: inline-block;
    padding: 0 4rpx;
    border-radius: 4rpx;
    margin: 0 10rpx 0 0;
    vertical-align: top;
    line-height: 35rpx;
    font-size: 24rpx;
    height: 35rpx;
}

.message-list .summary {
    display: inline-block;
    width: 540rpx;
    height: 35rpx;
    line-height: 35rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    font-size: 24rpx;
    white-space: nowrap;
    position: relative;
}

.white-wrap {
    background: #fff;
}

.recommend-box {
    padding: 0 30rpx;
}

.recommend-box .title {
    font-size: 28rpx;
    padding: 30rpx 0;
    text-align: center;
    color: #1684ff;
}

.recommend-box .line {
    display: inline-block;
    width: 200rpx;
    height: 2rpx;
    background: url('https://iservice.daqisoft.cn/Public/Home/images/newimages/index_title_icon_01.png') no-repeat;
    background-size: 200rpx 2rpx;
    transform: rotate(180deg);
}

.recommend-box .icon {
    display: inline-block;
    width: 8rpx;
    height: 22rpx;
    margin: 0 20rpx;
    background: url('https://iservice.daqisoft.cn/Public/Home/images/newimages/index_title_icon_02.png') no-repeat;
    background-size: 8rpx 22rpx;
}

.recommend-box .reverse {
    transform: rotate(-360deg);
}

.recommend-box .line, .recommend-box .icon, .recommend-box text {
    vertical-align: middle;
}

.recommend-box image {
    display: block;
    width: 100%;
    height: 100%;
    border-radius: 10rpx;
}

.image-box-1, .image-box-2 {
    font-size: 22rpx;
    position: relative;
    border-radius: 10rpx;
}

.image-box-2 {
    width: 353rpx;
    height: 151rpx;
    float: right;
}

.image-box-1 {
    width: 317rpx;
    height: 322rpx;
    float: left;
}

.image-box-3 {
    margin: 20rpx 0 0 20rpx;
}

.recommend-box .tag {
    position: absolute;
    color: #fff;
    background: #1684ff;
    border-radius: 10rpx 0 10rpx 0;
    padding: 5rpx 10rpx;
    z-index: 1;
}

.recommend-box .summary {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 20rpx;
    color: #fff;
    text-align: center;
}

.group-discount-title {
    line-height: 50rpx;
    padding: 0 0 0 15rpx;
    width:158rpx;
    height:50rpx;
    margin: 40rpx 0 30rpx;
    background: linear-gradient(to right bottom,#ffa03b,#ff8400);
    border-radius: 0 25rpx 25rpx 0;
    color: #fff;
    font-size: 28rpx;
}

.group-discount-list {
    padding: 0 30rpx;
    height: 265rpx !important;
}

.group-discount-list .item {
    float: left;
    width: 186rpx !important;
}

.group-discount-list image {
    display: block;
    width: 176rpx;
    height: 143rpx;
    border-radius: 10rpx;
}

.group-discount-list text {
    display: inline-block;
}

.group-discount-list text:nth-of-type(1) {
    width: 50%;
    font-size: 22rpx;
    color: #343434;
    text-align: left;
    margin: 10rpx 0 0 0;
}

.group-discount-list text:nth-of-type(2) {
    box-sizing: border-box;
    width: 50%;
    font-size: 18rpx;
    color: #00affb;
    text-align: right;
    padding: 0 10rpx 0 0;
}

.group-discount-list text:nth-of-type(3) {
    font-size: 18rpx;
    color: #999;
    margin: 20rpx 10rpx 0 0;
    text-decoration: line-through;
}

.group-discount-list text:nth-of-type(4) {
    font-size: 22rpx;
    color: #ff1d02;
}

.group-activity-title {
    line-height: 50rpx;
    padding: 0 0 0 15rpx;
    width:158rpx;
    height:50rpx;
    margin: 40rpx 0 30rpx;
    background: linear-gradient(to right bottom,#ff594c,#ff3323);
    border-radius: 0 25rpx 25rpx 0;
    color: #fff;
    font-size: 28rpx;
}

.group-activity {
    margin: 0rpx auto 20rpx;
    width: 690rpx;
    height: 240rpx;
    border-radius: 10rpx;
    box-shadow: 2rpx 2rpx 2rpx 2rpx rgba(0, 0, 0, 0.1);
    display: flex;
    overflow: hidden;
    position: relative;
}

.group-activity image {
    width: 300rpx;
    height: 240rpx;
    border-radius: 10rpx 0 0 10rpx;
}

.center-box {
    flex: 1;
    padding: 0 0 0 14rpx;
}

.center-box text:nth-of-type(1) {
    display: block;
    font-size: 32rpx;
    color: #333;
    line-height: 80rpx;
}

.center-box text:nth-of-type(2) {
    display: block;
    font-size: 22rpx;
    color: #999;
    line-height: 40rpx;
}

.center-box text:nth-of-type(3) {
    display: block;
    font-size: 16rpx;
    color: #999;
}

.user-image-list {
    margin: 30rpx 0 0 0;
    position: relative;
}

.user-image-list image {
    width: 50rpx;
    height: 50rpx;
    margin: 0 0 0 -25rpx;
    border-radius: 50%;
}

.user-image-list image:first-child {
    margin: 0;
}

.right-box {
    text-align: right;
    width: 150rpx;
    padding: 0 20rpx 0 0;
}

.right-box button {
    display: inline-block;
    font-size: 22rpx;
    color: #fff;
    width: 90rpx;
    line-height: 40rpx;
    background: linear-gradient(to right bottom,#ff7065,#ff3323);
    background-size: 90rpx 40rpx;
    text-align: center;
    outline: 0;
    padding: 0;
    margin: 20rpx 0 0 0;
    border-radius: 20rpx;
}

.right-box > text:nth-of-type(1) {
    display: inline-block;
    font-size: 18rpx;
    border: #0092fa 1rpx solid;
    border-radius: 6rpx;
    line-height: 30rpx;
    padding: 0 8rpx;
    color: #0092fa;
    margin: 4rpx 10rpx 0 0;
    text-align: center;
}

.price {
    position: absolute;
    bottom:10rpx;
    right:0rpx;
    z-index:99;
    width:200rpx;
}

.price text:nth-of-type(1) {
    display: inline-block;
    font-size: 24rpx;
    line-height: 50rpx;
    padding: 0 8rpx;
    color: #f60001;
    text-align: center;
}

.price text:nth-of-type(2) {
    display: inline-block;
    font-size: 16rpx;
    line-height: 50rpx;
    color: #999;
    text-decoration: line-through;
}

/* 点击领券 */

.couponmask {
    background: rgba(0, 0, 0, 0.6);
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index:4;
}

.coupon {
    position: fixed;
    left:50%;
    top:40%;
    transform: translateX(-50%) translateY(-50%);
    z-index:5;
}

.coupon image {
    width: 405rpx;
    height: 442rpx;
}

.coupon .exdesc {
    font-size: 26rpx;
    color: #caa876;
    text-align: center;
    position: absolute;
    left:0;
    right:0;
    margin-top:-80rpx;
}

.coupon .close {
    position: absolute;
    margin-top: -200rpx;
}

.coupon .close image {
    width: 80rpx;
    height: 80rpx;
    margin: 0 50rpx 0 380rpx;
    color: #fff;
}

/* 呼叫服务 */

.mask {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    z-index: 2;
    background: rgba(0, 0, 0, 0.8);
}

.call-service {
    width: 100%;
    position: fixed;
    top: 50%;
    transform: translateY(-50%);
    left: 0;
    z-index: 3;
    display: flex;
}

.donald-duck {
    position: absolute;
    bottom: 0;
    left: 0;
    padding: 0 0 0 20rpx;
    width: 226rpx;
    height: 459rpx;
    animation-fill-mode: forwards;
    -webkit-animation-fill-mode: forwards;
    animation-duration: 0.3s;
    -webkit-animation-duration: 0.3s;
    transform: translateX(-262rpx);
    -webkit-transform: translateX(-262rpx);
}

.donaldshowIn {
    animation-name: donaldshowIn;
    -webkit-animation-name: donaldshowIn;
}

@keyframes donaldshowIn {
    0% {
        transform: translateX(-262rpx);
        -webkit-transform: translateX(-262rpx);
    }

    100% {
        transform: translateX(0rpx);
        -webkit-transform: translateX(0rpx);
    }
}

@-webkit-keyframes donaldshowIn {
    0% {
        transform: translateX(-262rpx);
        -webkit-transform: translateX(-262rpx);
    }

    100% {
        transform: translateX(0rpx);
        -webkit-transform: translateX(0rpx);
    }
}

.donaldshowOut {
    animation-name: donaldshowOut;
    -webkit-animation-name: donaldshowOut;
}

@keyframes donaldshowOut {
    0% {
        transform: translateX(0rpx);
        -webkit-transform: translateX(0rpx);
    }

    100% {
        transform: translateX(-262rpx);
        -webkit-transform: translateX(-262rpx);
    }
}

@-webkit-keyframes donaldshowOut {
    0% {
        transform: translateX(0rpx);
        -webkit-transform: translateX(0rpx);
    }

    100% {
        transform: translateX(-262rpx);
        -webkit-transform: translateX(-262rpx);
    }
}

.donald-duck image {
    width: 100%;
    height: 100%;
}

.donald-duck-con {
    float: right;
    width: 488rpx;
    max-height: 765rpx;
    animation-fill-mode: forwards;
    -webkit-animation-fill-mode: forwards;
    animation-duration: 0.3s;
    -webkit-animation-duration: 0.3s;
    transform: translateX(488rpx);
    -webkit-transform: translateX(488rpx);
}

.donaldconshowIn {
    animation-name: donaldconshowIn;
    -webkit-animation-name: donaldconshowIn;
}

@keyframes donaldconshowIn {
    0% {
        transform: translateX(488rpx);
        -webkit-transform: translateX(488rpx);
    }

    100% {
        transform: translateX(0rpx);
        -webkit-transform: translateX(0rpx);
    }
}

@-webkit-keyframes donaldconshowIn {
    0% {
        transform: translateX(488rpx);
        -webkit-transform: translateX(488rpx);
    }

    100% {
        transform: translateX(0rpx);
        -webkit-transform: translateX(0rpx);
    }
}

.donaldconshowOut {
    animation-name: donaldconshowOut;
    -webkit-animation-name: donaldconshowOut;
}

@keyframes donaldconshowOut {
    0% {
        transform: translateX(0rpx);
        -webkit-transform: translateX(0rpx);
    }

    100% {
        transform: translateX(488rpx);
        -webkit-transform: translateX(488rpx);
    }
}

@-webkit-keyframes donaldconshowOut {
    0% {
        transform: translateX(0rpx);
        -webkit-transform: translateX(0rpx);
    }

    100% {
        transform: translateX(488rpx);
        -webkit-transform: translateX(488rpx);
    }
}

.donald_duck_title {
    width: 319rpx;
    height: 40rpx;
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
}

.service-box {
    margin: 60rpx 0 0 0;
}

.service-box scroll-view {
    max-height: 646rpx;
}

.service-list {
    width: 100%;
}

.service-list view {
    font-size: 30rpx;
    color: #ffd08d;
    height: 115rpx;
    line-height: 115rpx;
    text-align: left;
}

.service-list image {
    width: 70rpx;
    height: 70rpx;
    margin: 22rpx 25rpx 0 0;
}

.close-btn {
    position: absolute;
    width: 60rpx;
    height: 60rpx;
    left: 50%;
    transform: translateX(-50%);
    top: -140rpx;
}

.notice-box {
    width: 600rpx;
    min-height: 100rpx;
    max-height: 900rpx;
    overflow-y: auto;
    position: fixed;
    transform: translateY(-50%);
    top: 40%;
    left: 50%;
    margin-left: -300rpx;
