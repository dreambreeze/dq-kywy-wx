}

.close-qrcode {
    position: absolute;
    right: 0;
    top: 0;
    width: 60rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    font-size: 44rpx;
    color: #aaa9a9;
}

.qrcode-title {
    position: relative;
    text-align: center;
    color: #999;
    padding: 40rpx 0 26rpx;
    font-size: 26rpx;
}
