<import src="../common/engin-nav.wxml" />
<view class="container" style="min-height:{{minHeight}}">
    <!--banner轮播图 -->
    <view class='swiper'>
        <swiper indicator-dots="{{indicatorDots}}" autoplay="true" interval="3500" duration="800" circular="true" previous-margin="120rpx" next-margin="120rpx" bindchange='bindchange' current-item-id="{{selected}}">
            <block wx:for="{{bannerList}}" wx:key="bannerList">
                <swiper-item item-id="{{item.staffworkno}}">
                    <image src="{{item.picurl}}" class="slide-image {{item.staffworkno==selected?'active':''}} " />
                </swiper-item>
            </block>
        </swiper>
        <view class='contact-tools'>
            <view class='address-icon' bindtap="location"></view>
            <text bindtap="location">{{locationStore[0].address}}</text>
            <text>{{locationStore[0].distance}}</text>
            <view class='nav-icon' bindtap="location"></view>
            <text>|</text>
            <view class='tel-icon' data-phone="{{locationStore[0].tel}}" bindtap="phoneCall"></view>
        </view>
    </view>
    <!--服务入口列表-->
    <view class="function-list">
        <view class='item' bindtap='scancode'>
            <image src='../../images/index_features_01@2x.png' lazy-load></image>
            <view>扫码下单</view>
        </view>
        <view class='item' bindtap="showService">
            <image src='../../images/index_features_02@2x.png' lazy-load></image>
            <view>呼叫服务</view>
        </view>
        <view class='item'>
            <navigator url='../technician/pages/techindex/techindex'>
                <image src='../../images/index_features_03@2x.png' lazy-load></image>
                <view>预约技师</view>
            </navigator>
        </view>
        <view class='item'>
            <navigator url="../reserve/pages/reserve-room/reserve-room">
                <image src='../../images/index_features_04@2x.png' lazy-load></image>
                <view>房间预约</view>
            </navigator>
        </view>
        <view class='item'>
            <navigator url="../reserve/pages/store-assess/store-assess">
                <image src='../../images/index_features_05@2x.png' lazy-load></image>
                <view>店面评价</view>
            </navigator>
        </view>
    </view>
    <!-- 资讯列表 -->
    <view class="message-box">
        <image src='../../images/index_message@2x.png' lazy-load></image>
        <swiper class="message-list" vertical="true" display-multiple-items="2" duration="500" interval="3000" circular="true" autoplay="true">
            <swiper-item class="item">
                <text class="tag red-tag">最新</text>
                <text class="summary">自古中医就有云：万物皆生于春，长于夏，收于秋天，藏于冬,自古中医就有云：万物皆生于春，长于夏，收于秋天，藏于冬</text>
            </swiper-item>
            <swiper-item class="item">
                <text class="tag green-tag">公告</text>
                <text class='summary'>养生茶水养生按摩足浴</text>
            </swiper-item>
            <swiper-item class="item">
                <text class="tag red-tag">最新</text>
                <text class="summary">自古中医就有云：万物皆生于春，长于夏，收于秋天，藏于冬,自古中医就有云：万物皆生于春，长于夏，收于秋天，藏于冬</text>
            </swiper-item>
            <swiper-item class="item">
                <text class="tag green-tag">公告</text>
                <text class='summary'>养生茶水养生按摩足浴</text>
            </swiper-item>
        </swiper>
    </view>
    <view class="white-wrap">
        <!-- 精品推荐 -->
        <view class="recommend-box">
            <view class="title">
                <view class='line'></view>
                <view class='icon'></view>
                <text>精品推荐</text>
                <view class='icon'></view>
                <view class='line reverse'></view>
            </view>
            <view class="clear">
                <view class="image-box-1">
                    <text class='tag'>技师推荐</text>
                    <swiper display-multiple-items="1" duration="500" interval="3000" circular="true" autoplay="true" style="height:100%">
                        <swiper-item>
                            <image src='../../images/index_recommend_01@2x.png' lazy-load></image>
                            <text class='summary'>获得高级按摩师认证</text>
                        </swiper-item>
                        <swiper-item>
                            <image src='../../images/index_recommend_02@2x.png' lazy-load></image>
                            <text class='summary'>好口碑项目推荐</text>
                        </swiper-item>
                        <swiper-item>
                            <image src='../../images/index_recommend_03@2x.png'></image>
                            <text class='summary'>会员充值 乐享优惠</text>
                        </swiper-item>
                    </swiper>
                </view>
                <view class="image-box-2" bindtap='toReserveProject'>
                    <text class='tag'>项目介绍</text>
                    <image src='../../images/index_recommend_02@2x.png' lazy-load></image>
                    <text class='summary'>好口碑推荐项目</text>
                </view>
                <view class="image-box-3">
                    <text class='tag'>会员优惠</text>
                    <image src='../../images/index_recommend_03@2x.png' lazy-load></image>
                    <text class='summary'>会员充值 乐享优惠</text>
                </view>
            </view>
        </view>
        <!-- 团购优惠 -->
        <navigator url='../transbuy/pages/group-buy/group-buy'>
            <view class="group-discount-title">团购优惠</view>
            <swiper class="group-discount-list" indicator-dots="{{indicatorDots}}" autoplay="true" interval="3500" duration="800" circular="true" display-multiple-items="3">
                <swiper-item class="item">
