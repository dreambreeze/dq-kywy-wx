                </swiper-item>
                <swiper-item class="item">
                    <image src='../../images/group_discout_02@2x.png' lazy-load></image>
                    <text>泰式足道</text>
                    <text>70分钟</text>
                    <text>￥120</text>
                    <text>￥100</text>
                </swiper-item>
                <swiper-item class="item">
                    <image src='../../images/group_discout_01@2x.png' lazy-load></image>
                    <text>益健足浴</text>
                    <text>60分钟</text>
                    <text>￥120</text>
                    <text>￥100</text>
                </swiper-item>
                <swiper-item class="item">
                    <image src='../../images/group_discout_03@2x.png' lazy-load></image>
                    <text>养生按摩</text>
                    <text>90分钟</text>
                    <text>￥100</text>
                    <text>￥80</text>
                </swiper-item>
