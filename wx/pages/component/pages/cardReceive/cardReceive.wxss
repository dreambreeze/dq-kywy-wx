.form-box {
    padding: 0 30rpx 25rpx;
    background: #fff;
}

.form-item {
    display: flex;
    box-sizing: border-box;
    padding: 15rpx 0;
}

.form-item label {
    min-width: 146rpx;
    line-height: 50rpx;
    color: #333;
    font-size: 28rpx;
    text-align: right;
    margin: 0 20rpx 0 0;
}

.form-item input {
    flex: 1;
    margin: 0 48rpx 0 0;
    line-height: 50rpx;
    font-size: 28rpx;
    border-radius: 25rpx;
    border: #999 1rpx solid;
    padding: 0 25rpx;
}

.form-item .picker, .form-item picker {
    box-sizing: border-box;
    line-height: 50rpx;
    font-weight: 400;
    flex: 1;
    font-size: 28rpx;
}

.form-item .picker {
    margin: 0 48rpx 0 0;
    border-radius: 25rpx;
    border: #999 1rpx solid;
    padding: 0 15rpx;
}

.form-item .unpicker {
    color: #808080;
}

.sex-box view {
    width: 70rpx;
    height: 70rpx;
    margin: 0 25rpx;
    border-radius: 50%;
    padding: 0;
}

.sex-box label {
    line-height: 70rpx;
}

