}
.selfModal {
  width: 600rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  margin-left: -300rpx;
  background-color: #fff;
  border-radius: 6rpx;
  box-shadow: 0 0 20rpx rgba(0, 0, 0, 0.4);
}

.content-modal {
  padding: 24rpx 52rpx;
}

.modal-title {
  padding: 20rpx 0;
  color: #000;
  font-size: 38rpx;
  text-align: center;
}

.btn-modal {
  height: 96rpx;
  line-height: 96rpx;
  color: #3cc51f;
  border-top: 1rpx solid #f4f4f5;
  text-align: center;
  display: block;
}

.modal-contents-plabel {
  width: 126rpx;
}

.content-tips {
