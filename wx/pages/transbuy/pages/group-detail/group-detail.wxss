@import "../../../common/store-address.wxss";
@import "../../../common/assess.wxss";

.container {
    background-color: #eee;
    padding-bottom: 100rpx;
}

.banner swiper {
    height: 340rpx;
}

.item-image {
    width: 100%;
    height: 100%;
}

.project-info {
    padding: 25rpx 20rpx 0;
    margin-bottom: 12rpx;
    background: #fff;
}

.project-msg {
    font-size: 26rpx;
    color: #333;
    margin-bottom: 10rpx;
}

.project-msg text{
    vertical-align: middle;
}

.project-name{
    font-size: 32rpx;
    font-weight: bold;
}

.peoples {
    padding: 0 10rpx;
    border-radius: 8rpx;
    font-size: 22rpx;
    color: #0092f8;
    margin: 0 16rpx;
    border: #0092f8 1rpx solid;
    height: 28rpx;
    line-height: 28rpx;
}

.label{
    display: inline-block;
    color:#333;
    font-size: 24rpx;
    background: #ffe400;
    border-radius: 4rpx;
    line-height: 40rpx;
    padding: 0 10rpx;
    position: relative;
}

.label:before{
    content: '';
    width: 0;
    height: 0;
    border-right: #ffe400 10rpx solid;
    border-top:transparent 10rpx solid;
    border-bottom:transparent 10rpx solid;
    position: absolute;
    left: -8rpx;
    top:10rpx;
}

.order-num{
    float: right;
    font-size:26rpx;
    color:#999;
    margin-top:8rpx;
}

.project-price text:nth-of-type(1) {
    font-size: 38rpx;
    color: #e30101;
}

.project-price text:nth-of-type(1) text {
    font-size: 30rpx;
    margin-right: 8rpx;
}

.project-price text:nth-of-type(2) {
    font-size: 24rpx;
    color: #919090;
    margin-left: 22rpx;
}

.project-price text:nth-of-type(3) {
    float: right;
    font-size: 26rpx;
    color: #919090;
    margin-top: 10rpx;
}

.project-dil {
    background: #fff;
    padding: 0 20rpx;
    margin: 12rpx 0;
}

.dil-title {
    font-size: 32rpx;
    color: #3b81d6;
    line-height: 70rpx;
    display: flex;
    align-items: center;
    font-weight: bold;
    position: relative;
    padding: 0 10rpx;
}

.dil-title:before {
    content: '';
    width:10rpx;
    height:32rpx;
    background:#3b81d6;
    position: absolute;
    left:-20rpx;
    top:50%;
    transform: translateY(-52%);
}   

.techdetail {
    padding-bottom: 20rpx;
}

.td-intro {
    padding: 10rpx 20rpx 2rpx 20rpx;
}

.td-head {
    height: 50rpx;
    font-size: 26rpx;
    color: #f8ab11;
    display: flex;
    align-items: center;
    margin-left: -8rpx;
}

.td-head view {
    width: 10rpx;
    height: 25rpx;
    /* background-color:#caa876; */
    background-image: linear-gradient(to right bottom, #f8ab11, #caa876);
    margin-right: 8rpx;
    border-radius: 8rpx;
}

.td-desc {
    margin-top: 10rpx;
    line-height: 36rpx;
    font-size: 26rpx;
    position: relative;
    padding: 0 0 0 8rpx;
}

.td-desc:before{
    content:'';
    display: inline-block;
    width: 6rpx;
    height: 6rpx;
    border-radius: 50%;
    background: #999;
    position: absolute;
    left: -6rpx;
    top:15rpx;
}

.totips .dil-content{
    margin-top: 10rpx;
    line-height: 36rpx;
    font-size: 26rpx;
    /* text-indent: 8rpx; */
}

.dil-content {
    font-size: 24rpx;
    color: #333;
    padding-bottom: 20rpx;
}

.look-all {
    width: 180rpx;
    height: 46rpx;
    line-height: 46rpx;
    text-align: center;
    font-size: 24rpx;
    color: #3b81d6;
    border: 1rpx solid #3b81d6;
    background: #fff;
    margin: 30rpx auto;
    border-radius: 6rpx;
}

.buy-bar {
    width: 100%;
    height: 98rpx;
    background: #fff;
    position: fixed;
    bottom: 0;
    left: 0;
    box-sizing: border-box;
    text-align: center;
    font-size: 26rpx;
    z-index: 2;
}

.buy-bar .flex-wrp {
    justify-content: center;
    height: 98rpx;
}

.index {
    width: 28%;
    display: flex;
    justify-content: center;
    align-items: center;
    border-top: 1rpx solid rgba(0, 0, 0, 0.1);
    color: #3b81d6;
}

.onebuy, .buys {
    color: #fff;
    display: flex;
    justify-content: center;
    align-items: center;
    border-top: 1rpx solid rgba(0, 0, 0, 0.1);
    width: 36%;
}

.onebuy {
    background: #ff8400;
}

.buys {
    color: #fff;
    background: #ea3838;
}

.shidden {
    visibility: hidden;
}

.index image {
    width: 32rpx;
    height: 32rpx;
    margin: 0 10rpx 0 0;
}

/* 订单滚动 */

swiper {
    height: 242rpx;
}

.orders {
    margin-bottom: 16rpx;
    background-color: #fff;
    height: 242rpx;
}

.orders.oneorder, .orders.oneorder swiper {
    height: 122rpx;
}

swiper-item {
    height: 120rpx;
}

.orders .oitem {
    height: 120rpx;
    border-bottom: 1rpx solid rgba(0, 0, 0, 0.1);
    font-size: 26rpx;
}

.oitem .left {
    padding: 0 30rpx;
    flex: 2;
}

.oitem .mid {
    flex: 2;
}

.oitem .right {
    height: 120rpx;
    padding: 0 20rpx;
    flex: 1;
}

.left  image {
    width: 64rpx;
    height: 64rpx;
    border-radius: 50%;
    margin-left: -18rpx;
    border: 1rpx solid rgba(255, 255, 255, 1);
    box-shadow: 0 2rpx 8rpx 2rpx rgba(0, 0, 0, 0.1);
}

.left  image:first-child {
    width: 64rpx;
    height: 64rpx;
    border-radius: 50%;
    border: 1rpx solid rgba(255, 255, 255, 1);
}

.mid text {
    color: #e30101;
}

.right .gobuy {
    padding: 8rpx 8rpx;
    background-image: linear-gradient(to right bottom, #dfceb5, #caa776);
    width: 120rpx;
    color: #ffe;
    border-radius: 8rpx;
}


/* 联系方式 */
.contact-tools {
    display: flex;
    color: #656565;
    line-height: 80rpx;
    border-radius: 10rpx;
    font-size: 22rpx;
}

.address{
    flex:1;
    display: flex;
}

.address-icon {
    display: inline-block;
    width: 30rpx;
    height: 80rpx;
    background: url('https://note.youdao.com/yws/api/personal/file/WEB330ac01fc77c8bb32e84cb60c35ead81?method=download&shareKey=b31d7812be7686a3519fb7fdaeadc73f') no-repeat center left;
    background-size: 22rpx;
}

.address text:nth-of-type(1) {
    flex: 1;
    padding:0 10rpx 0 0;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.nav-icon {
    display: inline-block;
    width: 40rpx;
    height: 80rpx;
    background: url('https://note.youdao.com/yws/api/personal/file/WEB6afc3b4cf94f96b65bd55ba80fa769fe?method=download&shareKey=84ac7821f01df125479b4b2748f6fe93') no-repeat center;
    background-size: 22rpx;
}

.tel-icon {
    display: inline-block;
    width: 40rpx;
    height: 80rpx;
    background: url('https://note.youdao.com/yws/api/personal/file/WEBa516da57a01247147f37437f62dee584?method=download&shareKey=cb8e52113991cd5c2cfdd5e2529efeec') no-repeat center;
    background-size: 22rpx;
}