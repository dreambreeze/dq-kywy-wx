page{
  background: #eee;
}
.head {
  background-color: #fff;
  height: 340rpx;
  margin: 24rpx 34rpx;
  border-radius: 20rpx;
}
.head .h-img{
  display: flex;
  justify-content: center;
}
.h-img image{
  width: 126rpx;
  height: 138rpx;
}
.head .h-desc{
  color: #333;
  font-size: 32rpx;
  display: flex;
  align-items: center;
  margin-top: 38rpx;
  justify-content: center;
}
.head .h-time{
  color: #666;
  font-size: 26rpx;
  display: flex;
  align-items: center;
  margin-top: 46rpx;
  justify-content: center;
}
.h-time text{
  font-size: 28rpx;
  color: #ff8b24;
  padding: 0 6rpx;
}

.nav {
  display: flex;
  justify-content: center;
  font-size: 26rpx;
}
.nav view{
  height: 70rpx;
  width: 248rpx;
  border: 1rpx solid rgba(0, 0, 0, 0.3);
  border-radius: 8rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #666;
  margin: 0 40rpx ;
}
.nav view:last-child{
   border: 1rpx solid #caa876;
   background-color: #e6e3d6;
}

.ad-box{
  margin-top: 104rpx;
}
