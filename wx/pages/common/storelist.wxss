
.store-item {
    color: #fff;
    text-align: center;
    padding: 22rpx 5rpx;
}

.store-item text{
    font-size: 26rpx;
    line-height: 1.6;
    width: 180rpx;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    word-break: break-all;
}

.store-item.active {
    background: #265389;
}
