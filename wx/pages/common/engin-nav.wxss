.engin-nav {
  width: 100%;
  height: 100rpx;
  background: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  border-top: 1rpx solid #d6d6d6;
  box-sizing: border-box;
}

.engin-item {
  width: 25%;
  text-align: center;
  font-size: 20rpx;
  color: #7d7d7d;
}

.engin-item view {
  line-height: 1;
}
.engin-item view.active{
  color: #368aed
}

.engin-item:nth-of-type(1) image,.engin-item:nth-of-type(2) image {
  width: 40rpx;
  height: 38rpx;
  margin: 15rpx 0 8rpx;
}

.engin-item:nth-of-type(3) image,.engin-item:nth-of-type(4) image {
  width: 40rpx;
  height: 38rpx;
  margin: 15rpx 0 8rpx;
}