<template name="storelist">
    <scroll-view scroll-y enable-back-to-top>
        <block wx:for="{{dcStore}}" wx:key='index'>
            <view class="s-item {{storeIdx == index ? 'active' : ''}}" bindtap="changeStore" data-index="{{index}}">{{item}}</view>
        </block>
    </scroll-view>
</template>